Cell["Construct the measurements for error-syndrome detection.", "Text",
 CellChangeTimes->{{3.894771496949248*^9, 3.894771513361337*^9}},
 CellID->1552813743,ExpressionUUID->"96a5251e-b9ec-4a8f-88ea-3c466a1b742e"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"msr", "=", 
   RowBox[{"QuantumCircuit", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"QuantumCircuit", "@@@", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Measurement", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"1", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"2", ",", "3"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"2", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"3", ",", "3"}], "]"}]}]}], "}"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Measurement", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"4", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"5", ",", "3"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"5", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"6", ",", "3"}], "]"}]}]}], "}"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Measurement", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"7", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"8", ",", "3"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"8", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"9", ",", "3"}], "]"}]}]}], "}"}]}]}], 
       "\[IndentingNewLine]", "}"}]}], ",", 
     RowBox[{"{", "\"\<Spacer\>\"", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Measurement", "[", 
      RowBox[{"Multiply", "@@", 
       RowBox[{"S", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"1", ",", "6"}], "]"}], ",", "1"}], "]"}]}], "]"}], ",", 
     RowBox[{"{", "\"\<Spacer\>\"", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Measurement", "[", 
      RowBox[{"Multiply", "@@", 
       RowBox[{"S", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"4", ",", "9"}], "]"}], ",", "1"}], "]"}]}], "]"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"qc3", "=", 
  RowBox[{"QuantumCircuit", "[", 
   RowBox[{"qc2", ",", "\"\<Separator\>\"", ",", "msr"}], "]"}]}]}], "Input",
