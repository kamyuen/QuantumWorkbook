 CellID->1728871295,ExpressionUUID->"e3208030-f918-47f0-98ed-49a4f7ee3dad"],

Cell[BoxData[
 RowBox[{
  RowBox[{"bitFlip", "[", 
   RowBox[{"ss", ":", 
    RowBox[{"{", 
     RowBox[{"__", "?", "QubitQ"}], "}"}]}], "]"}], ":=", 
  RowBox[{"Apply", "[", "\[IndentingNewLine]", 
   RowBox[{"QuantumCircuit", ",", "\[IndentingNewLine]", 
    RowBox[{"ReleaseHold", "@", 
     RowBox[{"Thread", "[", 
      RowBox[{
       RowBox[{"Hold", "[", "CNOT", "]"}], "[", 
       RowBox[{
        RowBox[{"First", "@", "ss"}], ",", 
        RowBox[{"Rest", "[", "ss", "]"}]}], "]"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{3.915337108663656*^9},
 CellLabel->"In[226]:=",
 CellID->1479792433,ExpressionUUID->"708a9605-2813-4230-b878-9403a63d660e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"phaseFlip", "[", 
   RowBox[{"ss", ":", 
    RowBox[{"{", 
     RowBox[{"__", "?", "QubitQ"}], "}"}]}], "]"}], ":=", 
  RowBox[{"QuantumCircuit", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"bitFlip", "[", "ss", "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Through", "[", 
     RowBox[{"ss", "[", "6", "]"}], "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
