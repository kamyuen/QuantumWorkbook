 CellID->843946282,ExpressionUUID->"1e3ad754-63c7-4413-a919-0df04d3d712b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"out", "=", 
   RowBox[{"qc3", "**", "in"}]}], ";"}]], "Input",
