 CellID->1350242134,ExpressionUUID->"0965df21-5f01-48b0-bd6f-11f4ef8eb0d8"],

Cell[BoxData[
 RowBox[{"gnr", "=", 
  RowBox[{"Flatten", "[", 
   RowBox[{"msr", "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"QuantumCircuit", "->", "List"}], ",", 
      RowBox[{"Measurement", "->", "Identity"}], ",", 
      RowBox[{"\"\<Spacer\>\"", "->", "Nothing"}]}], "}"}]}], 
   "]"}]}]], "Input",
