 CellID->1640536447,ExpressionUUID->"b81adb65-c5b1-4e61-8268-b7e9cbaf456c"],

Cell[BoxData[
 RowBox[{"opL", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<+\>\"", "]"}], "]"}], " ", 
     RowBox[{"S", "[", "4", "]"}]}], ",", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<-\>\"", "]"}], "]"}], 
     RowBox[{"S", "[", "5", "]"}]}]}], "}"}]}]], "Input",
 CellLabel->"In[44]:=",
 CellID->682224499,ExpressionUUID->"e824f49b-3fa4-4b47-9d0d-75c44226c81c"],

Cell["Set the initial state.", "Text",
