Cell[TextData[{
 StyleBox["Theorem",
  FontWeight->"Bold"],
 ": Any supermap ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", ":", 
     RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalV]", ")"}]}], 
    "\[Rule]", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalW]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"9f552f31-6b67-4957-8745-a9dc66907717"],
 " can be written in the form"
}], "Text",
 CellChangeTimes->{{3.8943463241475153`*^9, 3.894346396874764*^9}},
 CellID->2041239938,ExpressionUUID->"1f1e26db-53dc-4796-8b0d-9b76c5060757"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "A", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]], 
     SubscriptBox["F", "\[Mu]"], "A", " ", 
     SubsuperscriptBox["F", "\[Nu]", "\[Dagger]"], 
     SubscriptBox["C", 
      RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]]}]}], TraditionalForm]],
  ExpressionUUID->"2d7a36c6-f69c-4e8f-991f-57375332bec6"],
 " ,"
}], "Text",
 CellChangeTimes->{{3.894346088680666*^9, 3.8943462012508*^9}, {
  3.894346418634317*^9, 3.894346418978765*^9}, {3.894346556804843*^9, 
