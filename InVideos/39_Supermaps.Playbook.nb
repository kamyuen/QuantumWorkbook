 CellID->78967015,ExpressionUUID->"a67cf848-3d53-4c1b-96c6-a4f88724158f"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], ":", "\[ScriptCapitalV]"}], "\[Rule]", 
    "\[ScriptCapitalW]"}], TraditionalForm]],ExpressionUUID->
  "4b0a0e22-a727-45dd-8b4a-6279fd0e9108"],
 " is a linear map from ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalV]", TraditionalForm]],ExpressionUUID->
  "a2731a05-a2af-45bd-8631-9426ade19fea"],
 " to ",
