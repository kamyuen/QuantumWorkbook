Cell[CellGroupData[{

Cell["Completely Positive Supermaps", "Section",
 CellChangeTimes->{{3.894346680065383*^9, 3.894346689289893*^9}},
 CellID->694792731,ExpressionUUID->"bacf7a0d-df77-4b71-931e-c74179e283fa"],

Cell[TextData[{
 StyleBox["Definition (positive)",
  FontWeight->"Bold"],
 ": A supermap ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", ":", 
     RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalV]", ")"}]}], 
    "\[Rule]", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalW]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"a169036c-a46a-48ba-b3a5-d3c1437c5435"],
 " is ",
 StyleBox["positive",
  FontSlant->"Italic"],
 " if ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalF]", TraditionalForm]],ExpressionUUID->
  "fd4ed74f-a93e-47ce-aec7-4c8a2f4e53e8"],
 " maps a positive operator to another positive operator; that is, \
positivity-preserving."
}], "Text",
 CellChangeTimes->{{3.894346731851125*^9, 3.894346848551494*^9}, {
  3.894346883714774*^9, 3.894346934877681*^9}, {3.894347583000825*^9, 
  3.89434762921032*^9}, {3.894347692076621*^9, 3.894347703536553*^9}},
 CellID->1174765854,ExpressionUUID->"0c298511-cccc-4f16-8a67-44ad59d5af24"],

Cell[TextData[{
 StyleBox["Definition (completely positive)",
  FontWeight->"Bold"],
 ": A positive supermap ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", ":", 
     RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalV]", ")"}]}], 
    "\[Rule]", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalW]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"cbb6bc56-cfb1-4579-a5ab-22525fd1cddf"],
 " is ",
 StyleBox["completely positive",
  FontSlant->"Italic"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalF]", "\[CircleTimes]", "\[ScriptCapitalI]"}], 
   TraditionalForm]],ExpressionUUID->"ffa8c118-ab99-4d47-96e6-10c94573e609"],
 " is positive on ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalV]", "\[CircleTimes]", "\[ScriptCapitalE]"}], 
   TraditionalForm]],ExpressionUUID->"5afc2fd0-d613-4339-a34b-5d06b6a57080"],
 " for all vector space ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalE]", TraditionalForm]],ExpressionUUID->
  "2e5c6cd7-d40b-46f7-aa87-d5a2b660905b"],
 "."
}], "Text",
 CellChangeTimes->{{3.894346731851125*^9, 3.894346848551494*^9}, {
  3.894346883714774*^9, 3.894346934877681*^9}, {3.894347595801302*^9, 
  3.894347599007641*^9}, {3.8943476483522253`*^9, 3.894347667650489*^9}, {
  3.894347851568054*^9, 3.894347873445382*^9}},
 CellID->7008390,ExpressionUUID->"63898fa7-2412-4cbd-81bf-86181ed2087b"],

Cell[TextData[{
 StyleBox["Theorem (Kraus)",
  FontWeight->"Bold"],
 ": Any ",
 StyleBox["completely positive supermap",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", ":", 
     RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalV]", ")"}]}], 
    "\[Rule]", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalW]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"c9ba0670-9dd7-42f5-8210-ed64acd30fa0"],
 " can be written in the form"
}], "Text",
 CellChangeTimes->{{3.8943463241475153`*^9, 3.894346396874764*^9}, {
   3.894346500427518*^9, 3.894346527643689*^9}, {3.8943465941730537`*^9, 
   3.894346639952529*^9}, {3.894346970488488*^9, 3.8943469720517397`*^9}, 
   3.894347387790735*^9},
 CellID->477892495,ExpressionUUID->"6ceb3e7f-b4b2-41ef-bfdf-4f0121ab89ff"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "A", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"\[Mu]", "\[VeryThinSpace]"}]], 
     SubscriptBox["F", "\[Mu]"], "A", " ", 
     SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"]}]}], TraditionalForm]],
  ExpressionUUID->"907da9f8-9fe5-414d-8af2-3908276a7a7e"],
 " ,"
}], "Text",
 CellChangeTimes->{{3.894346088680666*^9, 3.8943462012508*^9}, {
  3.894346418634317*^9, 3.894346418978765*^9}, {3.894346535502891*^9, 
  3.8943465715410833`*^9}, {3.894346648598064*^9, 3.8943466496101837`*^9}},
 CellID->350323685,ExpressionUUID->"c7c92735-05d9-4aee-8580-6d0595cfc4c8"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], ":", "\[ScriptCapitalV]"}], "\[Rule]", 
    "\[ScriptCapitalW]"}], TraditionalForm]],ExpressionUUID->
  "8fc9f1c6-abbb-44de-b2a9-96b109266a00"],
 " is a linear map from ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalV]", TraditionalForm]],ExpressionUUID->
  "2761731a-6bad-47d6-91ec-2ffc6caee191"],
 " to ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalW]", TraditionalForm]],ExpressionUUID->
  "d118e09d-6db7-4407-9529-3c74db7b03c5"],
 ". This is called the ",
 StyleBox["Kraus representation theorem",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.894346420664291*^9, 3.8943464646379023`*^9}, {
  3.894346977800262*^9, 3.89434698595767*^9}},
