Cell[TextData[{
 StyleBox["Definition (Hermitian)",
  FontWeight->"Bold"],
 ": A supermap ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", ":", 
     RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalV]", ")"}]}], 
    "\[Rule]", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalW]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"3ca30213-4bbe-4470-b7cb-c3a88abeac1c"],
 " is ",
 StyleBox["Hermitian",
  FontSlant->"Italic"],
 " if ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalF]", TraditionalForm]],ExpressionUUID->
  "d0657d60-e137-41df-b09b-3faade1b8762"],
 " maps a Hermitian operator to another Hermitian operator; that is, \
Hermiticity-preserving."
}], "Text",
 CellChangeTimes->{{3.894346731851125*^9, 3.894346848551494*^9}, {
  3.894346883714774*^9, 3.894346934877681*^9}, {3.894347583000825*^9, 
  3.89434762921032*^9}, {3.894347692076621*^9, 3.894347757893553*^9}, {
  3.89434779846418*^9, 3.894347798997056*^9}},
 CellID->107989128,ExpressionUUID->"e0843714-51f5-4e7a-bb70-675d1b278276"],

Cell[TextData[{
 StyleBox["Theorem",
  FontWeight->"Bold"],
 ": Any ",
 StyleBox["Hermitian supermap",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", ":", 
     RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalV]", ")"}]}], 
    "\[Rule]", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[ScriptCapitalW]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"f5838b3e-b880-48dd-b0a4-0337885a6040"],
 " can be written in the form"
}], "Text",
 CellChangeTimes->{{3.8943463241475153`*^9, 3.894346396874764*^9}, {
  3.894346500427518*^9, 3.894346527643689*^9}, {3.8943465941730537`*^9, 
  3.894346616892997*^9}, {3.894347544453561*^9, 3.894347554401775*^9}},
 CellID->1370636369,ExpressionUUID->"589e0497-de54-4a65-81de-84b95be51e39"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "A", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"\[Mu]", "\[VeryThinSpace]"}]], 
     SubscriptBox["F", "\[Mu]"], "A", " ", 
     SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"], 
     SubscriptBox["c", 
      RowBox[{"\[Mu]", "\[VeryThinSpace]"}]]}]}], TraditionalForm]],
  ExpressionUUID->"aba5b0e3-8014-4102-bbe3-5237470c881b"],
 " ,"
}], "Text",
