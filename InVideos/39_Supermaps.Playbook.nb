Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], ":", "\[ScriptCapitalV]"}], "\[Rule]", 
    "\[ScriptCapitalW]"}], TraditionalForm]],ExpressionUUID->
  "c884f3f3-7a76-4c61-b895-a83ea2974ef9"],
