 CellID->350323685,ExpressionUUID->"3c821edd-f028-42b1-94b7-42cc95203594"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], ":", "\[ScriptCapitalV]"}], "\[Rule]", 
    "\[ScriptCapitalW]"}], TraditionalForm]],ExpressionUUID->
  "fb84229d-3bf7-4139-ad78-04b17b5cbd69"],
