Cell[TextData[{
 "We are given a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "1"}], "}"}], "n"], "\[RightArrow]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"9e38740e-fd10-40b0-8eab-4aa53ce5d2c0"],
 " and a secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "c8acef09-c800-401d-9158-f374f28ffa9f"],
 " of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "44555355-8bd7-4324-8bdc-45e604de3030"],
 " bits. "
}], "ItemNumbered",
 CellChangeTimes->{{3.852070435901077*^9, 3.852070716088048*^9}, 
   3.855686665351173*^9, 3.893985211575436*^9, {3.899200862329848*^9, 
   3.899200877092928*^9}},
 CellID->776887413,ExpressionUUID->"3ef6deb2-2d62-4df7-addb-71b633cb216d"],

Cell[TextData[{
 "For all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", ",", 
    RowBox[{"y", "\[Element]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"345c8c84-95a1-42aa-8023-81bd4b3bf1c1"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{"f", 
     RowBox[{"(", "y", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "e9ac0e1e-07f3-4702-bd0f-bd9baf1a35dc"],
 " if and only if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"x", "\[CirclePlus]", "s"}]}], TraditionalForm]],ExpressionUUID->
  "5c709dab-c695-461b-a832-8aabf2ff58fc"],
 ". Note that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "a06257ce-475b-463e-9f36-7fffb11dd5e5"],
 " is either one-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "489efe93-9f63-4fb0-a3da-492f245b900e"],
 ") or two-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "59dff2f8-1c11-4b1c-a30a-1e8f600d7334"],
 "). "
}], "ItemNumbered",
 CellChangeTimes->{{3.852070435901077*^9, 3.852070716088048*^9}, 
   3.855686665351173*^9, 3.893985211575436*^9, {3.899200862329848*^9, 
   3.899200862952688*^9}, 3.899200893918707*^9},
 CellID->169035843,ExpressionUUID->"4447a33b-a00e-425a-86c5-19124af77e8e"],

Cell[TextData[{
 "The task is to find the secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "cd9288e0-10d0-4630-85b5-4930a590e2ee"],
 " with as few queries to function ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "a9d1a255-8a74-4c92-b5a7-872f511646d1"],
 " as possible. "
}], "ItemNumbered",
