Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"0"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"0"},
      "Ket"]}], 
    OverscriptBox["\[RightArrow]", 
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]]], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     RowBox[{
      TemplateBox[{"x"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"0"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "bd1d85be-c066-4f14-a9e8-9ea0bdcfa343"],
 "."
}], "Text",
 CellChangeTimes->{{3.852071295311932*^9, 3.85207131160734*^9}, {
  3.852071349026947*^9, 3.8520714143334274`*^9}, {3.8939859761943502`*^9, 
  3.8939859761944*^9}},
 CellID->1976242829,ExpressionUUID->"f71ecc99-371e-46b0-af2f-fb151beab391"],

