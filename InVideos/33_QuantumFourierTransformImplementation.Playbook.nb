Cell[TextData[{
 StyleBox["Figure 1",
  FontWeight->"Bold"],
 ". Implementation of quantum Fourier transform (QFT) by means of elementary \
gates, where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "k"], TraditionalForm]],ExpressionUUID->
  "4af89ec1-c8aa-4606-93db-c25c7d4d8cc4"],
 " denotes the phase gate by phase angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Pi]", "/", 
    SuperscriptBox["2", "k"]}], TraditionalForm]],ExpressionUUID->
  "a96af37b-082b-4548-80c6-c584857f69cd"],
 "; that is,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "k"], ":=", 
    RowBox[{"(", GridBox[{
       {"1", "0"},
       {"0", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          RowBox[{"\[Pi]", "/", 
           SuperscriptBox["2", "k"]}]}]]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "0c40a73b-639a-4244-9bb2-ffe234ad49dd"],
 " ."
}], "Text",
 CellChangeTimes->{{3.914905945776187*^9, 3.914906083288509*^9}},
 CellID->1373366348,ExpressionUUID->"98bf4ac2-4456-4be2-adc0-7f0852ea9a91"]
}, Closed]],

