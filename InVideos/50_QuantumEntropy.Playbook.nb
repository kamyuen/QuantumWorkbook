Cell[CellGroupData[{

Cell["Example", "Subsection",
 CellChangeTimes->{{3.915657477442595*^9, 3.915657478505306*^9}},
 CellID->790454356,ExpressionUUID->"ca0121e1-f891-4adc-af14-7704dcaea5b0"],

Cell[TextData[{
 "Consider a density operator describes an ensemble of two pure states ",
 Cell[BoxData[
  TemplateBox[{"0"},
   "Ket"]],ExpressionUUID->"2c48e8b3-4596-49a1-b90e-20d5abca1bc3"],
 " and ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{"+"},
    "Ket"], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      TemplateBox[{"0"},
       "Ket"], "+", 
      TemplateBox[{"1"},
       "Ket"]}], ")"}], "/", 
    SqrtBox["2"]}]}]],ExpressionUUID->
  "6adefe93-1fc1-4003-b923-54a8c58a0925"],
 "."
}], "Text",
 CellChangeTimes->{{3.83926730377583*^9, 3.839267385558619*^9}, {
  3.839267675995208*^9, 3.839267689437354*^9}, {3.915657486189394*^9, 
  3.915657495432292*^9}},
 CellID->1058952787,ExpressionUUID->"f0998de7-2ecf-4ce7-ac37-f76946beb923"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "p", "]"}], "\[IndentingNewLine]", 
 RowBox[{"ens", "=", 
  RowBox[{"Association", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Ket", "[", 
      RowBox[{"S", "->", "0"}], "]"}], "->", "p"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Ket", "[", 
         RowBox[{"S", "->", "0"}], "]"}], "+", 
        RowBox[{"Ket", "[", 
         RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
      RowBox[{"Sqrt", "[", "2", "]"}]}], "->", 
     RowBox[{"1", "-", "p"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellChangeTimes->{{3.8392673923570023`*^9, 3.839267489503368*^9}, {
  3.885091322327434*^9, 3.8850913315384407`*^9}, {3.885091406706219*^9, 
  3.885091408333132*^9}, {3.915657529451626*^9, 3.915657531845009*^9}, {
  3.915657563292228*^9, 3.915657625671658*^9}},
 CellLabel->"In[120]:=",
 CellID->1622508420,ExpressionUUID->"d15765cb-1fb8-4c23-b3c6-1c7fb306d70a"],

Cell[BoxData[
 RowBox[{"rho", "=", 
  RowBox[{"Total", "@", 
   RowBox[{"KeyValueMap", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"#1", "**", 
       RowBox[{"Dagger", "[", "#1", "]"}], "**", "#2"}], "&"}], ",", "ens"}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.915657642934033*^9, 3.915657663137305*^9}},
 CellLabel->"In[127]:=",
 CellID->1331036665,ExpressionUUID->"367e277e-acd7-456e-9847-7a7fd1a5b261"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Matrix", "[", "rho", "]"}], "//", "MatrixForm"}]], "Input",
 CellChangeTimes->{{3.915657667612894*^9, 3.915657671976132*^9}},
 CellLabel->"In[128]:=",
 CellID->358687412,ExpressionUUID->"260d338d-f3fe-44c9-b134-bf22be6a69f1"],

Cell["Examine the eigenvalues of the density operator.", "Text",
 CellChangeTimes->{{3.839267623516022*^9, 3.839267632954385*^9}, {
  3.91565751093014*^9, 3.915657514384901*^9}},
 CellID->751665334,ExpressionUUID->"9a9f92a1-2142-4dcb-9b78-cca18eefec84"],

Cell[BoxData[
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"ProperValues", "[", "rho", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"p", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"FrameLabel", "->", 
    RowBox[{"{", 
     RowBox[{"\"\<p\>\"", ",", "\"\<Eigenvalues\>\""}], "}"}]}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.839267553389182*^9, 3.839267611299501*^9}},
 CellLabel->"In[129]:=",
 CellID->1740456884,ExpressionUUID->"633055c9-4cfe-45f9-a568-9da6573aa0b1"],

Cell[TextData[{
 "Ploit the von Neumann entropy of the density operator as a function of ",
 StyleBox["p",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.839267636588201*^9, 3.8392676627538567`*^9}, {
  3.915657685994553*^9, 3.915657693345418*^9}},
 CellID->984093601,ExpressionUUID->"8b12b3c9-78a5-4300-b45a-41743e14f215"],

Cell[BoxData[
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"VonNeumannEntropy", "[", "rho", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"p", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"FrameLabel", "->", 
    RowBox[{"{", 
     RowBox[{"\"\<p\>\"", ",", "\"\<S(\[Rho])\>\""}], "}"}]}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.839267508751823*^9, 3.839267546165786*^9}, 
   3.8392676197934723`*^9},
 CellLabel->"In[130]:=",
 CellID->1889586460,ExpressionUUID->"6f0db64f-d21f-4d32-abcf-aa02693168e5"]
}, Closed]]
}, Closed]],

Cell[CellGroupData[{

Cell["Quantum Relative Entropy", "Section",
 CellChangeTimes->{{3.9111837602630053`*^9, 3.911183776384255*^9}, {
  3.915657715990439*^9, 3.91565771954151*^9}},
 CellID->982443254,ExpressionUUID->"3264e9fc-cb7a-4c74-89f4-41bfab394e22"],

Cell[TextData[{
 "Let \[Rho] and \[Sigma] be two density operators on a vector space. We \
define the relative von Neumann entropy or simply ",
 StyleBox["relative entropy",
  FontSlant->"Italic"],
 " of \[Rho] to \[Sigma] by"
}], "Item",
 CellChangeTimes->{{3.908967731272326*^9, 3.908967750078261*^9}},
 CellID->260405746,ExpressionUUID->"f9d8dcfe-1439-4569-b747-d8532ad2596c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", 
     RowBox[{"\[Rho]", "\[DoubleVerticalBar]", "\[Sigma]"}], ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"Tr", "[", 
       RowBox[{
       "\[Rho]", "\[VeryThinSpace]", "log", "\[VeryThinSpace]", "\[Rho]"}], 
       "]"}], "-", 
      RowBox[{"Tr", "[", 
       RowBox[{
       "\[Rho]", "\[VeryThinSpace]", "log", "\[VeryThinSpace]", "\[Sigma]"}], 
       "]"}]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"S", "(", "\[Rho]", ")"}]}], "-", 
      RowBox[{"Tr", "[", 
       RowBox[{
       "\[Rho]", "\[VeryThinSpace]", "log", "\[VeryThinSpace]", "\[Sigma]"}], 
       "]"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "58167512-b1ed-4480-84cb-758101cc4129"],
 " ."
}], "Text",
 CellChangeTimes->{{3.9089677674652*^9, 3.908967856217683*^9}},
 CellID->89979787,ExpressionUUID->"f17e6369-e116-474a-9153-6fffdf815ec3"],

Cell["\<\
One can regard it as a measure of the closeness of two quantum states.\
\>", "Item",
 CellChangeTimes->{{3.9156578610222178`*^9, 3.915657870447542*^9}},
 CellID->2047829298,ExpressionUUID->"f97e4883-e9c1-47de-8ef1-dc0ff509af42"],

Cell[CellGroupData[{

Cell["Example", "Subsection",
 CellChangeTimes->{{3.915657905904825*^9, 3.915657906635766*^9}},
 CellID->871925644,ExpressionUUID->"1a2537c5-5252-400b-97b1-9da632964b5b"],

Cell["\<\
Consider a quantum state described by the following density operator.\
\>", "Text",
 CellChangeTimes->{{3.839270249296102*^9, 3.8392702711205873`*^9}},
 CellID->831059677,ExpressionUUID->"1bd614ec-8b80-4ffd-aa3f-0b99cbe864bd"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"rho", "=", 
   RowBox[{
    RowBox[{"1", "/", "2"}], "+", 
    RowBox[{
     RowBox[{"S", "[", "3", "]"}], "/", "4"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rho", "//", "Matrix"}], "//", "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.839269326920681*^9, 3.839269327356676*^9}, {
   3.839269450616103*^9, 3.839269503589436*^9}, 3.83926957364068*^9},
 CellLabel->"In[131]:=",
 CellID->386351251,ExpressionUUID->"d89bb058-83c1-472e-b7c5-63531365be10"],

Cell["Calculate the von Neumann entropy of the above state.", "Text",
 CellChangeTimes->{{3.8392702754970284`*^9, 3.839270289209175*^9}, {
  3.915657917586999*^9, 3.915657922874619*^9}},
 CellID->1562410676,ExpressionUUID->"98e9e705-bc7f-4ced-bd52-98c292544725"],

Cell[BoxData[
 RowBox[{"VonNeumannEntropy", "[", "rho", "]"}]], "Input",
 CellChangeTimes->{{3.839269326920681*^9, 3.839269327356676*^9}, {
   3.839269450616103*^9, 3.839269503589436*^9}, 3.83926957364068*^9},
 CellLabel->"In[133]:=",
 CellID->6196016,ExpressionUUID->"2d8eb3cf-1577-4f0f-aaf0-1802d37d3cc9"],

Cell["Consider another density operator.", "Text",
 CellChangeTimes->{{3.839270294327729*^9, 3.839270302460896*^9}},
 CellID->1057225856,ExpressionUUID->"4882794a-dbd7-471e-817f-4b6dd12ab47d"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"sgm", "=", 
   RowBox[{
    RowBox[{"S", "[", "6", "]"}], "**", "rho", "**", 
    RowBox[{"S", "[", "6", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sgm", "//", "Matrix"}], "//", "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.839269512634351*^9, 3.839269578094291*^9}, {
  3.871593444661277*^9, 3.871593447574628*^9}},
 CellLabel->"In[134]:=",
 CellID->515266514,ExpressionUUID->"853d72c3-b78c-4822-b903-632eddb864ab"],

Cell[TextData[{
 "It has the same eigenvalues as ",
 StyleBox["rho", "Input"],
 " and hence the same von Neumann entropy."
}], "Text",
 CellChangeTimes->{{3.839270306645493*^9, 3.839270326321281*^9}},
 CellID->1430236094,ExpressionUUID->"59e8bfc1-3f5d-4f62-a9f2-b78b272db616"],

Cell[BoxData[
 RowBox[{"VonNeumannEntropy", "[", "sgm", "]"}]], "Input",
 CellChangeTimes->{{3.8392695799638424`*^9, 3.839269583954991*^9}, {
  3.871593450331716*^9, 3.871593450769721*^9}},
 CellLabel->"In[136]:=",
 CellID->1193570987,ExpressionUUID->"a4a9318e-8d19-4781-8477-2bd1921afb0a"],

Cell[TextData[{
 "Calculate the relative entropy of ",
 StyleBox["rho", "Input"],
 " to ",
 StyleBox["sgm", "Input"],
 "."
}], "Text",
 CellChangeTimes->{{3.839270335623555*^9, 3.839270355231495*^9}, {
  3.871593455258747*^9, 3.871593456551873*^9}, {3.9156579402848783`*^9, 
  3.915657941810621*^9}},
 CellID->702831882,ExpressionUUID->"c41c24e4-2a6b-4810-9b82-d9dcaeab86ce"],

Cell[BoxData[
 RowBox[{"VonNeumannEntropy", "[", 
  RowBox[{"rho", ",", "sgm"}], "]"}]], "Input",
 CellChangeTimes->{{3.839269587220929*^9, 3.83926959452955*^9}, {
  3.8715934589268293`*^9, 3.871593459917091*^9}},
 CellLabel->"In[137]:=",
 CellID->1335382274,ExpressionUUID->"74284dd7-e45d-483e-86d0-22d45013ba5f"],

Cell["Let us consider still another density operator.", "Text",
 CellChangeTimes->{{3.871592912258501*^9, 3.871592926584457*^9}, {
  3.8715934926206303`*^9, 3.871593500214244*^9}, {3.871632887034048*^9, 
  3.87163289046776*^9}},
 CellID->1167270714,ExpressionUUID->"2d21a302-8bf5-42b8-a850-5723c7e05f41"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"tau", "=", 
   RowBox[{
    RowBox[{"1", "/", "2"}], "+", 
    RowBox[{
     RowBox[{"S", "[", "2", "]"}], "/", "2"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tau", "//", "Matrix"}], "//", "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.871591121963426*^9, 3.871591131244272*^9}, {
  3.87159116294783*^9, 3.871591172105532*^9}, {3.871593337253652*^9, 
  3.87159335733534*^9}},
 CellLabel->"In[138]:=",
 CellID->2110866041,ExpressionUUID->"d335f9b1-409f-49fb-93a0-5d22520a2d3f"],

Cell["Note that it has one zero eigenvalue.", "Text",
 CellChangeTimes->{{3.871592932412229*^9, 3.871593045230884*^9}, {
  3.87159311023482*^9, 3.871593112653681*^9}},
 CellID->1844018300,ExpressionUUID->"5c52a843-81a5-41f3-863b-05afbabafcdb"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
