Cell[CellGroupData[{

Cell["What is it?", "Section",
 CellID->1852245629,ExpressionUUID->"19bf6ef6-eb88-4fcb-921c-c7850e23a89b"],

Cell["\<\
The Shannon entropy is a measure of uncertainty associated with a classical \
probability distribution. The von Neumann entropy generalizes the notion to \
quantum states described by density operators.\
\>", "Item",
 CellChangeTimes->{
  3.908776609666311*^9, {3.915657324219808*^9, 3.915657335550049*^9}},
 CellID->2046238193,ExpressionUUID->"7d4605e7-08ee-408c-b043-396e4b064de8"],

Cell[TextData[{
 "The ",
 StyleBox["von Neumann entropy",
  FontSlant->"Italic"],
 " of a quantum state ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]],ExpressionUUID->
  "eb1a8a8f-2e65-4b21-bc3d-793907f1bacd"],
 " is defined by"
}], "Item",
 CellChangeTimes->{{3.908918328613489*^9, 3.908918333185383*^9}},
 CellID->943524637,ExpressionUUID->"754c206a-8547-4dc6-be0b-c288eee78186"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "\[Rho]", ")"}], ":=", 
    RowBox[{"\[Minus]", 
     RowBox[{"Tr", " ", "[", 
      RowBox[{"\[Rho]", " ", "log", "\[VeryThinSpace]", "\[Rho]"}], "]"}]}]}],
    TraditionalForm]],ExpressionUUID->
  "9ab8ae67-956c-459a-8221-b7a1e471a34f"],
 " "
}], "Text",
 CellChangeTimes->{{3.908918342000542*^9, 3.9089183792789803`*^9}, 
   3.915673153251357*^9},
 CellID->354454418,ExpressionUUID->"d32f503e-9595-4a16-9c61-3b4110bd7292"],

Cell["The logarithm is with respect to base 2. ", "Item",
 CellChangeTimes->{{3.908918389709481*^9, 3.908918404227371*^9}, 
   3.91565738310772*^9, 3.91567314720498*^9},
 CellID->1045765700,ExpressionUUID->"a69f1a01-08a1-499f-8ddf-4618b153d6db"],

Cell[CellGroupData[{

Cell["How to calculate?", "Subsection",
 CellChangeTimes->{{3.915657385372517*^9, 3.915657391302281*^9}},
 CellID->1929559405,ExpressionUUID->"6bacbdc0-6be7-4043-81c6-f49bac74f5c1"],

Cell["\<\
A basic approach to evaluate the above expression is to take the spectral \
decomposition\
\>", "Text",
 CellChangeTimes->{{3.908918389709481*^9, 3.908918404227371*^9}},
 CellID->1613901946,ExpressionUUID->"acf147f0-3ec9-4081-8103-73dbe18302cc"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "\[Lambda]"], 
     TemplateBox[{"\[Lambda]"},
      "Ket"], "\[Lambda]", 
     TemplateBox[{"\[Lambda]"},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "40926a93-ac8c-4dd1-8849-4d140dbf059d"]
}], "Text",
 CellChangeTimes->{{3.908918421172647*^9, 3.908918453670385*^9}},
 CellID->562472759,ExpressionUUID->"c3bbba68-6e68-409e-b60c-2d542a3f9bed"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"\[Lambda]"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "c8984e98-41da-4bd2-a95a-0108b0fc08e5"],
 " is the eigenvector of ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]],ExpressionUUID->
  "d3e3c238-bb37-4bcd-9803-fdfe50dab2b1"],
 " with eigenvalue ",
 Cell[BoxData[
  FormBox["\[Lambda]", TraditionalForm]],ExpressionUUID->
  "cf0006a6-74b8-4a97-a908-e0761e3c8be8"],
 ". "
}], "Text",
 CellChangeTimes->{{3.908918470251053*^9, 3.908918538675195*^9}, 
   3.9156574067039*^9},
 CellID->227605959,ExpressionUUID->"c427fd0e-6812-460d-9904-985f98adf30e"],

Cell[CellGroupData[{

Cell["\t", "Text", "ParagraphDelimiter",
 ShowCellBracket->Automatic,
 CellMargins->{{Inherited, Inherited}, {Inherited, 10}},
 CellGroupingRules->{"SectionGrouping", 70},
 TabFilling->"\[LongDash]\[NegativeThickSpace]",
 TabSpacings->Scaled[1],
 FontColor->GrayLevel[0.85],
 CellID->436341381,ExpressionUUID->"f1002f98-b776-4d6a-8805-b4cfe44536d4"],

Cell["Then, the von Neumann entropy is given by", "Text",
 CellChangeTimes->{{3.908918470251053*^9, 3.908918538675195*^9}, 
   3.915657418096594*^9},
 CellID->1352195032,ExpressionUUID->"2d01c2f7-0fbb-4e0a-b6e2-2b15d79d83c7"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     UnderscriptBox[
      RowBox[{"-", "\[CapitalSigma]"}], "\[Lambda]"], "\[Lambda]", 
     "\[VeryThinSpace]", "log", "\[VeryThinSpace]", "\[Lambda]"}]}], 
   TraditionalForm]],ExpressionUUID->"8b54b623-8c16-4e5e-96da-9fbf927e6e4a"],
 " ."
}], "Text",
 CellChangeTimes->{{3.9089185481891546`*^9, 3.9089185868682747`*^9}},
 CellID->1855553708,ExpressionUUID->"bd609790-3d40-4d8c-be01-c623bed45bf0"]
}, Open  ]],

