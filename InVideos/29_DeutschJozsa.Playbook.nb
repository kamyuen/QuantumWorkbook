Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"f", "(", "0", ")"}]], 
      SuperscriptBox["2", "n"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"y", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     SuperscriptBox[
      RowBox[{
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"x", "=", "0"}], 
        RowBox[{
         SuperscriptBox["2", "n"], "-", "1"}]], "(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"x", "\[CenterDot]", "y"}]]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"f", "(", "0", ")"}]], 
