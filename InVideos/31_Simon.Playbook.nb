Cell[TextData[{
 "We are given a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "1"}], "}"}], "n"], "\[RightArrow]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"23db1148-ab6a-4304-a10b-66cd990be129"],
 " and a secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "034b46c6-d9ec-4f99-b860-23a80c1c7fb9"],
 " of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "361f25a9-48d2-4ca9-8239-c2c4e6f568ba"],
 " bits. "
}], "ItemNumbered",
 CellChangeTimes->{{3.852070435901077*^9, 3.852070716088048*^9}, 
   3.855686665351173*^9, 3.893985211575436*^9, {3.899200862329848*^9, 
   3.899200877092928*^9}},
 CellID->776887413,ExpressionUUID->"20e79e82-a265-432f-9724-5c7f39adf040"],

Cell[TextData[{
 "For all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", ",", 
    RowBox[{"y", "\[Element]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"5133218c-b161-4ba0-9f56-fbcae019d809"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{"f", 
     RowBox[{"(", "y", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "3dee99f8-2f90-441c-83b6-7df7dd3cc085"],
 " if and only if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"x", "\[CirclePlus]", "s"}]}], TraditionalForm]],ExpressionUUID->
  "3d398e23-1794-43c8-92f4-ff76c72732f5"],
 ". Note that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "2120ba5c-2c52-4030-934c-7e79b619f57e"],
 " is either one-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "c34c53d3-9a23-4813-a94e-3286c98af8d7"],
 ") or two-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "ba081ecc-8ac5-42ad-8577-34ba9bfffa8d"],
 "). "
}], "ItemNumbered",
 CellChangeTimes->{{3.852070435901077*^9, 3.852070716088048*^9}, 
   3.855686665351173*^9, 3.893985211575436*^9, {3.899200862329848*^9, 
   3.899200862952688*^9}, 3.899200893918707*^9},
 CellID->169035843,ExpressionUUID->"7225be07-a63b-4c88-b883-a00a5f656dc8"],

Cell[TextData[{
 "The task is to find the secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "f6bf63e4-139c-4c51-a640-9ca9e4c247f3"],
 " with as few queries to function ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "cee14eb7-d751-4204-be36-f7a71302bbac"],
 " as possible. "
}], "ItemNumbered",
