Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"0"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"0"},
      "Ket"]}], 
    OverscriptBox["\[RightArrow]", 
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]]], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     RowBox[{
      TemplateBox[{"x"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"0"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "bc6c25d3-260d-43f3-8104-55471bfc2624"],
 "."
}], "Text",
 CellChangeTimes->{{3.852071295311932*^9, 3.85207131160734*^9}, {
