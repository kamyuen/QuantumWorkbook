Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"0"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"0"},
      "Ket"]}], 
    OverscriptBox["\[RightArrow]", 
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]]], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     RowBox[{
      TemplateBox[{"x"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"0"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "e4f98424-092a-4a31-8694-87c5a98df828"],
 "."
}], "Text",
 CellChangeTimes->{{3.852071295311932*^9, 3.85207131160734*^9}, {
  3.852071349026947*^9, 3.8520714143334293`*^9}, {3.8939859761943502`*^9, 
  3.8939859761944*^9}},
 CellID->1976242829,ExpressionUUID->"51bd779e-5672-4e68-ab15-05cacc2470de"],

