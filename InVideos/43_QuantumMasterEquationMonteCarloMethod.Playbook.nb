  FormBox[
   RowBox[{"data", "=", 
    RowBox[{"{", " ", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        TemplateBox[{
          SubscriptBox["\[Psi]", "in"]},
