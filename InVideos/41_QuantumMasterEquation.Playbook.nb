 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "non"], "(", "t", ")"}], ":=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", "t", 
      "\[VeryThinSpace]", 
      SubscriptBox["H", "non"]}]]}], TraditionalForm]],ExpressionUUID->
  "f35c0963-76c8-441e-830d-189d9eed734c"],
 " ."
}], "Text",
 CellFrameLabels->{{None, 
    Cell[
