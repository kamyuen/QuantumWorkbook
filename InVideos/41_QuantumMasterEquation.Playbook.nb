Cell[TextData[{
 "Integrating quantum master equation gets quickly intractable as the system \
increases in size. For example, for a system of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "2f826197-3ef9-40ec-948c-7834f2e62006"],
 " qubits, one has to deal with an order of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["4", "n"], "\[Times]", 
    SuperscriptBox["4", "n"]}], TraditionalForm]],ExpressionUUID->
  "edf06570-a2eb-4880-b894-6d10e7130e17"],
 "matrix. The required memory is simply too large for most available \
computers."
}], "Text",
 CellChangeTimes->{{3.8944886568806868`*^9, 3.89448866862607*^9}},
 CellID->2086703375,ExpressionUUID->"368de081-40c5-4bf0-8342-8849d46a5efe"],

Cell[TextData[{
 "For a short elapse ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"c2a34e59-9ca7-4eb2-991d-50d57f10d7c4"],
 " of time from ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]],ExpressionUUID->
  "24f8f494-6b20-43be-9eb1-6aedee680a15"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "+", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], TraditionalForm]],
  ExpressionUUID->"7e5df56d-1dc2-410f-904e-0f3d02de3d06"],
 ", up to the first order in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"518fe543-d57b-42b1-a40a-01d09c10418a"],
 ", the density operator evolves as"
}], "Text",
 CellChangeTimes->{3.894488671533061*^9},
 CellID->668184468,ExpressionUUID->"cca58acb-7a10-4e4f-83f5-95e1530ef21b"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["U", "non"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      RowBox[{
       SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"0a4f63f4-95fa-4cbe-b41c-0996ab160a35"],
 " ,"
}], "Text",
 CellFrameLabels->{{None, 
    Cell[
     TextData[{"(", 
       CounterBox["DisplayFormulaNumbered"], ")"}], 
     "DisplayFormulaEquationNumber"]}, {None, None}},
