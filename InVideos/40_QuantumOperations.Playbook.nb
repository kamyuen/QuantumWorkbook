 CellID->1345331145,ExpressionUUID->"ca0227d6-7eb0-491d-bde9-a0be6668d9a5"],

Cell[BoxData[
 RowBox[{"BlochSphere", "[", 
  RowBox[{"{", 
   RowBox[{"Red", ",", 
    RowBox[{"Bead", "[", 
     RowBox[{"BlochVector", "@", "out"}], "]"}]}], "}"}], "]"}]], "Input",
 CellChangeTimes->{{3.91507068303214*^9, 3.915070720552512*^9}, {
  3.915070760241243*^9, 3.915070761005341*^9}},
 CellLabel->"In[235]:=",
 CellID->1687077520,ExpressionUUID->"09a157c2-eb01-4b6d-ace6-1e3a96401e73"],

Cell["\<\
Apply the dephasing operation recursively and collect the results to form a \
trajectory of quantum states.\
\>", "Text",
