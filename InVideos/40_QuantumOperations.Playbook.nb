  RowBox[{"NestList", "[", 
   RowBox[{"spr", ",", "in", ",", "5"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.915070808786388*^9, 3.915070835865859*^9}},
 CellLabel->"In[236]:=",
 CellID->145164769,ExpressionUUID->"d444e68f-016d-4c61-b578-734936c6e815"],

