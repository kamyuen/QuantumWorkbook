 CellID->968617342,ExpressionUUID->"8fc25950-2fb7-4ec4-988e-920fa608ae2b"],

Cell[BoxData[
 RowBox[{"out", "=", 
  RowBox[{"spr", "[", "in", "]"}]}]], "Input",
 CellChangeTimes->{{3.915070732436727*^9, 3.915070772042137*^9}},
 CellLabel->"In[233]:=",
 CellID->1625622734,ExpressionUUID->"fe9fe4d1-5000-4c4f-9378-2c6bf08c8bd3"],

