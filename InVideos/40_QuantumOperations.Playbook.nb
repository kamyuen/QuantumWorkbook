Cell[TextData[{
 "Assume that the total system consisting of the system and environment is \
initially in the product state ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "\[CircleTimes]", "\[Sigma]"}], TraditionalForm]],
  ExpressionUUID->"bed81678-d68b-4b3a-8c44-83bca9083193"],
 "."
}], "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481222517706*^9}},
 CellID->1432144295,ExpressionUUID->"b4e0374e-a6d5-4dc3-9f9c-501fa3eae020"],

Cell["The total system is a closed system, hence,", "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481199965044*^9}, {
   3.8944812346600857`*^9, 3.894481242734629*^9}},
 CellID->996738344,ExpressionUUID->"d4ad5e89-250d-4368-808c-50fd8a0ec9b5"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], "\[RightTeeArrow]", 
    RowBox[{
     RowBox[{"U", "(", 
      RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], ")"}], 
     SuperscriptBox["U", "\[Dagger]"]}]}], TraditionalForm]],ExpressionUUID->
  "14519a07-b5a6-4e82-ae7e-b06f98b54802"],
 "."
}], "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481199965044*^9}, {
   3.8944812346600857`*^9, 3.894481268213736*^9}},
 CellID->1113187547,ExpressionUUID->"1b4e9de5-7d76-430f-b603-99d9fc6df8e6"],

Cell["\<\
 Without access to the environment, one has to take a partial trace of the \
final state over the environment to obtain the state of the system.\
\>", "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481199965044*^9}, {
   3.8944812346600857`*^9, 3.894481250567756*^9}, 3.894481289444437*^9},
 CellID->1480261761,ExpressionUUID->"5b1e7797-75f2-4684-a536-acce0063856f"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["Tr", "\[ScriptCapitalE]"], "[", 
      RowBox[{
       RowBox[{"U", "(", 
        RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], ")"}], 
       SuperscriptBox["U", "\[Dagger]"]}], "]"}], "=", 
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "i"], 
      TemplateBox[{
        SubscriptBox["\[CurlyEpsilon]", "i"]},
       "Bra"], "U", " ", 
      RowBox[{"(", 
       RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], ")"}], 
      SuperscriptBox["U", "\[Dagger]"], 
      TemplateBox[{
        SubscriptBox["\[CurlyEpsilon]", "i"]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "095100d6-d90f-41f1-8bef-0368f1d800b1"],
 ","
}], "Text",
 CellChangeTimes->{{3.894481300241777*^9, 3.8944813208340797`*^9}, {
  3.906423270917021*^9, 3.906423281181203*^9}},
 CellID->1109817324,ExpressionUUID->"adb74bca-abda-4bae-a15d-3d7d3927ee4f"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["\[CurlyEpsilon]", "i"]},
        "Ket"], "|", "i"}], "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
     SubscriptBox["d", "\[ScriptCapitalE]"]}], "}"}], TraditionalForm]],
  ExpressionUUID->"9b124eb8-d900-4342-9c45-f99309504a02"],
 " is an orthonormal basis of ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalE]", TraditionalForm]],ExpressionUUID->
  "3e8cf5c5-71cb-4722-ac8d-d5defdef351c"],
 " of dimension ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "\[ScriptCapitalE]"], TraditionalForm]],ExpressionUUID->
  "0e8d5fca-03de-44e1-9c9f-2ee5b41fbc15"],
 "."
}], "Text",
 CellChangeTimes->{3.8944813323425293`*^9},
 CellID->300876422,ExpressionUUID->"17403e24-ea8f-4be6-b200-1e6af4fb27c9"],

Cell[TextData[{
 "To construct the Kraus elements, we first take the spectral decomposition \
of ",
 Cell[BoxData[
  FormBox["\[Sigma]", TraditionalForm]],ExpressionUUID->
  "8f39aeac-677f-4493-91f4-379437fe14c7"]
}], "Text",
 CellChangeTimes->{{3.8944813428862658`*^9, 3.8944813519836903`*^9}},
 CellID->6107605,ExpressionUUID->"8febc10c-59cd-4272-b70e-a07f0d038f89"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Sigma]", "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"j", "=", "1"}], "r"], 
     TemplateBox[{
       SubscriptBox["s", "j"]},
      "Ket"], 
     TemplateBox[{
       SubscriptBox["s", "j"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "9971ae5e-bac3-47b5-bcc3-c2817803dc60"],
 ","
}], "Text",
 CellChangeTimes->{{3.894481355230521*^9, 3.894481355383234*^9}},
 CellID->163388462,ExpressionUUID->"0c3a927a-25eb-46a5-a41c-aedbb7f28f59"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],ExpressionUUID->
  "39a1c0cc-f848-4784-a5d3-e303d91490e1"],
 " is the rank of ",
 Cell[BoxData[
  FormBox["\[Sigma]", TraditionalForm]],ExpressionUUID->
  "a5bdb2a5-e700-4c08-8057-a7903decd28a"],
 ", and the eigenvectors ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     SubscriptBox["s", "j"]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "2d6bc08d-2ca8-478d-9b93-cb1dceae701c"],
 " of ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Sigma]", "^"], TraditionalForm]],ExpressionUUID->
  "8737b24c-618a-4379-ade7-89207b90fe1c"],
 " have been normalized by their own eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["s", "j"], "=", 
    TemplateBox[{
      SubscriptBox["s", "j"], 
      SubscriptBox["s", "j"]},
     "BraKet"]}], TraditionalForm]],ExpressionUUID->
  "d9278eb6-6640-4e77-8b08-1b7beacd667e"],
 "."
}], "Text",
 CellChangeTimes->{{3.894481364308808*^9, 3.894481390746682*^9}},
 CellID->498651953,ExpressionUUID->"7911c1f3-e7d8-4d3a-aeeb-9fb66a1c8e90"],

Cell[TextData[{
 "We then define linear maps ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["F", "^"], 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":", 
    RowBox[{"\[ScriptCapitalV]", "\[RightTeeArrow]", "\[ScriptCapitalW]"}]}], 
   TraditionalForm]],ExpressionUUID->"d48e4c66-a10a-4ad8-a3e6-f620ae1e5a3c"],
 " for each ",
 StyleBox["i",
  FontSlant->"Italic"],
 " and ",
 StyleBox["j",
  FontSlant->"Italic"],
 " by"
}], "Text",
 CellChangeTimes->{{3.894481364308808*^9, 3.8944813917805653`*^9}},
 CellID->1587271904,ExpressionUUID->"0ae65fe1-c789-4f0b-a206-f1db5f0ceffd"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["Tr", "\[ScriptCapitalE]"], "[", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"I", "\[CircleTimes]", 
           TemplateBox[{
             SubscriptBox["\[CurlyEpsilon]", "i"]},
            "Ket"]}], 
          TemplateBox[{
            SubscriptBox["s", "j"]},
           "Bra"]}], ")"}], "\[Dagger]"], "U"}], "]"}], "=", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["\[CurlyEpsilon]", "i"]},
       "Bra"], "U", 
      TemplateBox[{
        SubscriptBox["s", "j"]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "231bebdb-7264-4693-8ba3-cb54a4cacefd"],
 " "
}], "Text",
 CellChangeTimes->{{3.894481403450582*^9, 3.894481414924752*^9}, 
   3.8949438010940447`*^9},
 CellID->53268147,ExpressionUUID->"45b013b6-1115-4a2c-8041-071487292fa8"],

Cell["to get", "Text",
 CellChangeTimes->{{3.894943798916183*^9, 3.8949437995419083`*^9}},
 CellID->1064998890,ExpressionUUID->"ee22c617-037e-4628-83dc-143568991fcf"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     SubscriptBox["F", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], "\[Rho]", "\[VeryThinSpace] ", 
     
     SubsuperscriptBox["F", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}], "\[Dagger]"]}]}], 
   TraditionalForm]],ExpressionUUID->"c2845194-e84a-46fd-bdd1-ba5a36cf1da7"],
 " ."
}], "Text",
 CellChangeTimes->{{3.894481460424877*^9, 3.894481510787783*^9}, {
  3.894943808897571*^9, 3.894943825857909*^9}, {3.906423301455443*^9, 
  3.906423301718399*^9}},
 CellID->1165508081,ExpressionUUID->"5b44621d-7568-428d-bad6-b68a75e914ac"],

Cell[TextData[{
 "We finally regard ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", ":=", 
    RowBox[{"(", 
     RowBox[{"i", ",", "j"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "a964d76b-e899-4b50-9813-76bf66580f25"],
 " as a collective index and identify ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["F", "^"], "\[Mu]"], "\[Congruent]", 
    SubscriptBox[
     OverscriptBox["F", "^"], 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]]}], TraditionalForm]],
  ExpressionUUID->"553d4306-6834-4bfc-8def-45bd6507a50b"],
 " to be the desired Kraus elements:"
}], "Text",
 CellChangeTimes->{{3.894481460424877*^9, 3.894481510787783*^9}, 
   3.894483554843731*^9},
 CellID->1380274610,ExpressionUUID->"b0765090-ae70-41a0-8d24-752d7c3ace07"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "\[Mu]"], 
     SubscriptBox["F", "\[Mu]"], "\[Rho]", "\[VeryThinSpace] ", 
     SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"]}]}], TraditionalForm]],
  ExpressionUUID->"65000819-2cc9-479d-8756-f746d80f99f7"],
 " ."
}], "Text",
