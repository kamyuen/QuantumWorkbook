 CellLabel->"In[33]:=",
 CellID->714244653,ExpressionUUID->"8a722832-fcad-4450-9c3e-26d73c422132"],

Cell["The time-evolution operator of the chain is evaluated.", "Text",
 CellChangeTimes->{{3.831018301490602*^9, 3.831018332822888*^9}},
 CellID->39509992,ExpressionUUID->"6a1cf444-310e-4032-bc48-5894db447de9"],

Cell[BoxData[{
 RowBox[{"Let", "[", 
  RowBox[{"Real", ",", "t"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"U", "[", "t_", "]"}], "=", 
   RowBox[{"Elaborate", "@", 
    RowBox[{"MultiplyExp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], " ", "t", " ", "H"}], "]"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.831000938128545*^9, 3.831000966972871*^9}, {
  3.831014913093897*^9, 3.831014922101895*^9}, {3.8310151479075823`*^9, 
