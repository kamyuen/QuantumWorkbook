 CellID->1184789840,ExpressionUUID->"4e521dbc-5e74-4798-b78c-2f684e2a934e"],

Cell[BoxData[
 RowBox[{"spr", "=", 
  RowBox[{"Supermap", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"1", "-", "$p"}], "]"}], ",", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"$p", "/", "3"}], "]"}], "*", 
      RowBox[{"S", "[", "1", "]"}]}], ",", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"$p", "/", "3"}], "]"}], "*", 
      RowBox[{"S", "[", "2", "]"}]}], ",", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"$p", "/", "3"}], "]"}], "*", 
      RowBox[{"S", "[", "3", "]"}]}]}], "}"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.915070570009419*^9, 3.915070592518798*^9}, {
