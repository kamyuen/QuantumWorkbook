Cell["\<\
Now, suppose that we want to encode a single-qubit state to protect it \
against the bit-flip and phase-flip errors.\
\>", "Text",
 CellID->646562821,ExpressionUUID->"d55ff803-df11-43a2-a30a-f44d7802fe66"],

Cell[BoxData[
 RowBox[{"in", "=", 
  RowBox[{"ProductState", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"S", "[", "1", "]"}], "->", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c", "[", "0", "]"}], ",", 
       RowBox[{"c", "[", "1", "]"}]}], "}"}]}], ",", 
    RowBox[{"\"\<Label\>\"", "->", 
     RowBox[{"Ket", "[", "\[Psi]", "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.832292719756838*^9, 3.832292766888834*^9}, {
  3.832292887817581*^9, 3.832292893028821*^9}, {3.882212465403815*^9, 
  3.882212465444929*^9}},
 CellLabel->"In[87]:=",
 CellID->514078727,ExpressionUUID->"b71d2541-aecb-48f7-8cca-0fbc63ec9685"],

Cell["Calculate the output state.", "Text",
 CellChangeTimes->{{3.9082549414367948`*^9, 3.9082549551026278`*^9}},
 CellID->843946282,ExpressionUUID->"03b2c859-b668-4edf-8873-7234feb986c8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"out", "=", 
   RowBox[{"qc3", "**", "in"}]}], ";"}]], "Input",
 CellChangeTimes->{3.908254925038618*^9},
 CellLabel->"In[114]:=",
 CellID->1303105708,ExpressionUUID->"829fe90a-70c2-493e-a4e6-65d51607219c"],

Cell["Extract the observables in the measurements.", "Text",
 CellChangeTimes->{{3.908254960783494*^9, 3.908254977171606*^9}},
 CellID->1350242134,ExpressionUUID->"df857870-1060-449f-8ca6-2f3fa1414a4c"],

Cell[BoxData[
 RowBox[{"gnr", "=", 
  RowBox[{"Flatten", "[", 
   RowBox[{"msr", "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"QuantumCircuit", "->", "List"}], ",", 
      RowBox[{"Measurement", "->", "Identity"}], ",", 
      RowBox[{"\"\<Spacer\>\"", "->", "Nothing"}]}], "}"}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.9082547643341837`*^9, 3.908254851859928*^9}},
 CellLabel->"In[115]:=",
 CellID->851138189,ExpressionUUID->"b5aad09a-cbf4-42b6-bf51-9932c4cae263"],

Cell["\<\
Check the measurement outcomes, which tells you where the error occurs and \
what is the error.\
\>", "Text",
 CellChangeTimes->{{3.90825498563*^9, 3.908254992065734*^9}, {
  3.908255047694637*^9, 3.908255070389843*^9}},
 CellID->1492667055,ExpressionUUID->"1d63411c-315c-45da-84ff-217c6bc42ec4"],

Cell[BoxData[
 RowBox[{"sdr", "=", 
  RowBox[{"Readout", "/@", "gnr"}]}]], "Input",
 CellChangeTimes->{{3.882230557904427*^9, 3.88223056846772*^9}, {
   3.882230769187293*^9, 3.882230849062951*^9}, {3.8822308894697733`*^9, 
   3.88223096140631*^9}, {3.882231148905661*^9, 3.882231150403971*^9}, {
   3.908254886869835*^9, 3.90825488891842*^9}, 3.908254922117341*^9},
 CellLabel->"In[121]:=",
 CellID->443263550,ExpressionUUID->"ad7e36b7-39ce-4366-8ad3-819c312e09c1"],

Cell["Recover the original logical state .", "Text",
 CellChangeTimes->{{3.908255078512724*^9, 3.908255106559623*^9}},
 CellID->2050689917,ExpressionUUID->"9b798976-cb65-42bf-9188-94a82fca0104"],

Cell[BoxData[
 RowBox[{
  RowBox[{"new", "=", 
   RowBox[{"err", "**", "out"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.882230965268361*^9, 3.882230982189383*^9}, {
  3.882231034169184*^9, 3.882231039146919*^9}, {3.882231159872525*^9, 
  3.882231169116657*^9}, {3.882231256593175*^9, 3.882231267434765*^9}, {
  3.895531795107248*^9, 3.895531798332605*^9}},
