Cell[TextData[{
 "Assume that the total system consisting of the system and environment is \
initially in the product state ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "\[CircleTimes]", "\[Sigma]"}], TraditionalForm]],
  ExpressionUUID->"0c85e08e-9807-4549-9a7e-36637d5345ed"],
 "."
}], "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481222517706*^9}},
 CellID->1432144295,ExpressionUUID->"de3a8b4a-371e-4a9c-8906-1aa26fbfd70e"],

Cell["The total system is a closed system, hence,", "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481199965044*^9}, {
   3.8944812346600857`*^9, 3.894481242734629*^9}},
 CellID->996738344,ExpressionUUID->"636e2cd1-99b7-4860-8f36-8ce4d9d3ce9a"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], "\[RightTeeArrow]", 
    RowBox[{
     RowBox[{"U", "(", 
      RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], ")"}], 
     SuperscriptBox["U", "\[Dagger]"]}]}], TraditionalForm]],ExpressionUUID->
  "65560a22-ba48-4b2f-bbc7-386af1a65264"],
 "."
}], "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481199965044*^9}, {
   3.8944812346600857`*^9, 3.894481268213736*^9}},
 CellID->1113187547,ExpressionUUID->"d3b65bea-6d1c-4a65-85f7-ec296eebd16f"],

Cell["\<\
 Without access to the environment, one has to take a partial trace of the \
final state over the environment to obtain the state of the system.\
\>", "Text",
 CellChangeTimes->{
  3.8944799075646143`*^9, {3.894481176948122*^9, 3.894481199965044*^9}, {
   3.8944812346600857`*^9, 3.894481250567756*^9}, 3.894481289444437*^9},
 CellID->1480261761,ExpressionUUID->"79afebc4-cb79-4c26-ad68-906988a2cb7d"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["Tr", "\[ScriptCapitalE]"], "[", 
      RowBox[{
       RowBox[{"U", "(", 
        RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], ")"}], 
       SuperscriptBox["U", "\[Dagger]"]}], "]"}], "=", 
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "i"], 
      TemplateBox[{
        SubscriptBox["\[CurlyEpsilon]", "i"]},
       "Bra"], "U", " ", 
      RowBox[{"(", 
       RowBox[{"\[Rho]", "\[CircleTimes]", " ", "\[Sigma]"}], ")"}], 
      SuperscriptBox["U", "\[Dagger]"], 
      TemplateBox[{
        SubscriptBox["\[CurlyEpsilon]", "i"]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "71f549ce-5f30-447d-92f0-03c929679eb5"],
 ","
}], "Text",
 CellChangeTimes->{{3.894481300241777*^9, 3.8944813208340797`*^9}, {
  3.906423270917021*^9, 3.906423281181203*^9}},
 CellID->1109817324,ExpressionUUID->"61205e4e-af3d-43fe-b29b-61415b5f6864"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["\[CurlyEpsilon]", "i"]},
        "Ket"], "|", "i"}], "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
     SubscriptBox["d", "\[ScriptCapitalE]"]}], "}"}], TraditionalForm]],
  ExpressionUUID->"64cb9142-255c-4fac-bfbd-3e8272038461"],
 " is an orthonormal basis of ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalE]", TraditionalForm]],ExpressionUUID->
  "9f9933d5-d1fa-4301-b2e5-1aefe01d7f39"],
 " of dimension ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "\[ScriptCapitalE]"], TraditionalForm]],ExpressionUUID->
  "f10cc0d5-a29e-44bc-abb3-ad79fd707275"],
 "."
}], "Text",
 CellChangeTimes->{3.8944813323425293`*^9},
 CellID->300876422,ExpressionUUID->"eb9e918b-64b4-40ec-9a8d-abc6de33e7d9"],

Cell[TextData[{
 "To construct the Kraus elements, we first take the spectral decomposition \
of ",
 Cell[BoxData[
  FormBox["\[Sigma]", TraditionalForm]],ExpressionUUID->
  "c0499324-43ef-41fc-8cfe-300e5babb17f"]
}], "Text",
 CellChangeTimes->{{3.8944813428862658`*^9, 3.8944813519836903`*^9}},
 CellID->6107605,ExpressionUUID->"f9f1689a-9f39-4e0c-aa3b-80a034eb865b"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Sigma]", "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"j", "=", "1"}], "r"], 
     TemplateBox[{
       SubscriptBox["s", "j"]},
      "Ket"], 
     TemplateBox[{
       SubscriptBox["s", "j"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "7d173699-d6cb-4265-b592-67d8e1089974"],
 ","
}], "Text",
 CellChangeTimes->{{3.894481355230521*^9, 3.894481355383234*^9}},
 CellID->163388462,ExpressionUUID->"3e886443-1e92-4827-a017-9bea53e5ed93"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],ExpressionUUID->
  "fae1b17e-4764-4647-808a-8b6d9d0fc7ea"],
 " is the rank of ",
 Cell[BoxData[
  FormBox["\[Sigma]", TraditionalForm]],ExpressionUUID->
  "f33632c0-f9cf-4da4-8fa4-9ac360633ba8"],
 ", and the eigenvectors ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     SubscriptBox["s", "j"]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "5046aced-12b6-4003-a0a5-fa98f85f9548"],
 " of ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Sigma]", "^"], TraditionalForm]],ExpressionUUID->
  "3c44ea54-3728-453f-b68c-628d8e49ec68"],
 " have been normalized by their own eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["s", "j"], "=", 
    TemplateBox[{
      SubscriptBox["s", "j"], 
      SubscriptBox["s", "j"]},
     "BraKet"]}], TraditionalForm]],ExpressionUUID->
  "48fcef45-cd39-4237-837f-97ca79558e09"],
 "."
}], "Text",
 CellChangeTimes->{{3.894481364308808*^9, 3.894481390746682*^9}},
 CellID->498651953,ExpressionUUID->"cf5b9b62-0787-4644-abc3-af409c56f90c"],

Cell[TextData[{
 "We then define linear maps ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["F", "^"], 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":", 
    RowBox[{"\[ScriptCapitalV]", "\[RightTeeArrow]", "\[ScriptCapitalW]"}]}], 
   TraditionalForm]],ExpressionUUID->"54e5f409-f227-47be-ac0f-e80d096b5df2"],
 " for each ",
 StyleBox["i",
  FontSlant->"Italic"],
 " and ",
 StyleBox["j",
  FontSlant->"Italic"],
 " by"
}], "Text",
 CellChangeTimes->{{3.894481364308808*^9, 3.8944813917805653`*^9}},
 CellID->1587271904,ExpressionUUID->"ed8e6ae9-6956-4100-905f-b6ad5e866db9"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["Tr", "\[ScriptCapitalE]"], "[", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"I", "\[CircleTimes]", 
           TemplateBox[{
             SubscriptBox["\[CurlyEpsilon]", "i"]},
            "Ket"]}], 
          TemplateBox[{
            SubscriptBox["s", "j"]},
           "Bra"]}], ")"}], "\[Dagger]"], "U"}], "]"}], "=", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["\[CurlyEpsilon]", "i"]},
       "Bra"], "U", 
      TemplateBox[{
        SubscriptBox["s", "j"]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "f0aa29b8-3d04-4481-84f4-5ca446f092dc"],
 " "
}], "Text",
 CellChangeTimes->{{3.894481403450582*^9, 3.894481414924752*^9}, 
   3.8949438010940447`*^9},
 CellID->53268147,ExpressionUUID->"45cfee60-d6af-41d7-9e9a-d7c8f8bae90c"],

Cell["to get", "Text",
 CellChangeTimes->{{3.894943798916183*^9, 3.8949437995419083`*^9}},
 CellID->1064998890,ExpressionUUID->"3d0a2b2c-cbfb-44c9-bd90-e3a9143b4d8c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     SubscriptBox["F", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], "\[Rho]", "\[VeryThinSpace] ", 
     
     SubsuperscriptBox["F", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}], "\[Dagger]"]}]}], 
   TraditionalForm]],ExpressionUUID->"dc369121-4444-4172-9f25-8fe259602096"],
 " ."
}], "Text",
 CellChangeTimes->{{3.894481460424877*^9, 3.894481510787783*^9}, {
  3.894943808897571*^9, 3.894943825857909*^9}, {3.906423301455443*^9, 
  3.906423301718399*^9}},
 CellID->1165508081,ExpressionUUID->"197340f9-2b7c-4727-80d0-ffd23f8fc877"],

Cell[TextData[{
 "We finally regard ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", ":=", 
    RowBox[{"(", 
     RowBox[{"i", ",", "j"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "7e93dc50-e34d-478f-bd0d-59479ce244be"],
 " as a collective index and identify ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["F", "^"], "\[Mu]"], "\[Congruent]", 
    SubscriptBox[
     OverscriptBox["F", "^"], 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]]}], TraditionalForm]],
  ExpressionUUID->"5504e791-9007-4b72-acf5-7599222f5592"],
 " to be the desired Kraus elements:"
}], "Text",
 CellChangeTimes->{{3.894481460424877*^9, 3.894481510787783*^9}, 
   3.894483554843731*^9},
 CellID->1380274610,ExpressionUUID->"30637f27-0206-42ad-b4ed-c37855e042fc"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "\[Mu]"], 
     SubscriptBox["F", "\[Mu]"], "\[Rho]", "\[VeryThinSpace] ", 
     SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"]}]}], TraditionalForm]],
  ExpressionUUID->"2c7eef32-ab2b-4dd1-98c3-f17bed0d5e72"],
 " ."
}], "Text",
