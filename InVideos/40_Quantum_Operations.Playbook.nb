 CellID->39509992,ExpressionUUID->"abe6f080-e259-4fb4-89e3-a85b1a71b147"],

Cell[BoxData[{
 RowBox[{"Let", "[", 
  RowBox[{"Real", ",", "t"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"U", "[", "t_", "]"}], "=", 
   RowBox[{"Elaborate", "@", 
    RowBox[{"MultiplyExp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], " ", "t", " ", "H"}], "]"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.831000938128545*^9, 3.831000966972871*^9}, {
  3.831014913093897*^9, 3.831014922101895*^9}, {3.8310151479075823`*^9, 
