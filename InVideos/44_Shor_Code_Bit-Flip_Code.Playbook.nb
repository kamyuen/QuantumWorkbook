 CellID->2050689917,ExpressionUUID->"0b074c1d-e7f0-46dd-a245-c1eadef74ec6"],

Cell[BoxData[
 RowBox[{
  RowBox[{"new", "=", 
   RowBox[{"err", "**", "out"}]}], ";"}]], "Input",
