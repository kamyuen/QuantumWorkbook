    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<-\>\"", "]"}], "]"}], 
     RowBox[{"S", "[", "5", "]"}]}]}], "}"}]}]}], "Input",
 CellLabel->"In[3]:=",
 CellID->890120871,ExpressionUUID->"0db50aab-925d-4caf-a710-72e7ba647170"],

Cell["Take an initial state.", "Text",
 CellID->1184140969,ExpressionUUID->"269bf0ba-c207-409a-a0f2-f1a618664fb8"],

Cell[BoxData[
 RowBox[{"in", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Ket", "[", "]"}], "+", 
      RowBox[{"Ket", "[", 
       RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
    RowBox[{"Sqrt", "[", "2", "]"}]}], "//", "KetRegulate"}]}]], "Input",
