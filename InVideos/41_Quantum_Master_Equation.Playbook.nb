Cell[TextData[{
 "Integrating quantum master equation gets quickly intractable as the system \
increases in size. For example, for a system of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "bc40a213-2779-4373-8ea0-0c267e54faad"],
 " qubits, one has to deal with an order of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["4", "n"], "\[Times]", 
    SuperscriptBox["4", "n"]}], TraditionalForm]],ExpressionUUID->
  "4eb02b51-6f1b-4f32-b8c2-0520123afc34"],
 "matrix. The required memory is simply too large for most available \
computers."
}], "Text",
 CellChangeTimes->{{3.8944886568806868`*^9, 3.89448866862607*^9}},
 CellID->2086703375,ExpressionUUID->"f062ef0d-304e-4ef7-a24f-b62395bf559d"],

Cell[TextData[{
 "For a short elapse ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"435dee3d-2c17-4256-830a-44a16b5dc207"],
 " of time from ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]],ExpressionUUID->
  "fdeefdea-5b2b-466d-9ef4-cd6948b5840e"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "+", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], TraditionalForm]],
  ExpressionUUID->"280ce619-de69-44ef-b419-ce5eeee34a29"],
 ", up to the first order in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"46e7e2cb-635a-4da7-9b97-e6ca8f8c4fce"],
 ", the density operator evolves as"
}], "Text",
 CellChangeTimes->{3.894488671533061*^9},
 CellID->668184468,ExpressionUUID->"c2b90699-d158-4338-8ae8-25f0303bf497"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["U", "non"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      RowBox[{
       SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"173ed7df-2022-4e46-818d-8199ba947d5a"],
 " ,"
}], "Text",
 CellFrameLabels->{{None, 
    Cell[
     TextData[{"(", 
       CounterBox["DisplayFormulaNumbered"], ")"}], 
