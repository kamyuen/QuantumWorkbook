Cell[BoxData[
 RowBox[{"opH", "=", 
  RowBox[{"S", "[", "3", "]"}]}]], "Input",
 CellLabel->"In[2]:=",
 CellID->1225090936,ExpressionUUID->"95029700-6532-407b-9cc5-6929e945d41e"],

Cell["Consider the bit-flip noises as quantum jump operators.", "Text",
 CellID->2138209167,ExpressionUUID->"d2b632e9-6789-4071-b20d-c575dd64f5ad"],

Cell[BoxData[{
 RowBox[{"Let", "[", 
  RowBox[{"Real", ",", "\[CapitalGamma]"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"opL", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<+\>\"", "]"}], "]"}], " ", 
     RowBox[{"S", "[", "4", "]"}]}], ",", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<-\>\"", "]"}], "]"}], 
     RowBox[{"S", "[", "5", "]"}]}]}], "}"}]}]}], "Input",
 CellLabel->"In[3]:=",
 CellID->890120871,ExpressionUUID->"aebade6e-f09d-4244-9c8c-372b6249a1d9"],

Cell["Take an initial state.", "Text",
 CellID->1184140969,ExpressionUUID->"9009eecb-8a3b-4a80-82ac-9aeaaf77f13f"],

Cell[BoxData[
 RowBox[{"in", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Ket", "[", "]"}], "+", 
      RowBox[{"Ket", "[", 
       RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
    RowBox[{"Sqrt", "[", "2", "]"}]}], "//", "KetRegulate"}]}]], "Input",
 CellLabel->"In[5]:=",
 CellID->1126309234,ExpressionUUID->"175f3b63-39c3-4146-9e81-f9450ea1e74a"],

