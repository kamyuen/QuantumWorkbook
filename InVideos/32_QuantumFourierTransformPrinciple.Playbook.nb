Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["F", "0"], ",", 
      SubscriptBox["F", "1"], ",", "\[Ellipsis]"}], ")"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["f", "0"], ",", 
       SubscriptBox["f", "1"], ",", "\[Ellipsis]"}], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["D", "00"], 
         SubscriptBox["D", "01"], "\[Ellipsis]"},
        {
         SubscriptBox["D", "10"], 
         SubscriptBox["D", "11"], "\[Ellipsis]"},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]"}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "df05825a-da2b-414a-a83d-26e78b380999"],
 "  ,\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     RowBox[{"exp", "(", 
      FractionBox[
       RowBox[{
       "2", "\[Pi]", "\[VeryThinSpace]", "\[ImaginaryI]", " ", "i", 
        "\[VeryThinSpace]", "j"}], "N"], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"6b524734-b7b6-4a22-ba3a-272583b9539c"]
}], "Text",
 CellChangeTimes->{{3.9149032325044403`*^9, 3.9149033358284793`*^9}, {
