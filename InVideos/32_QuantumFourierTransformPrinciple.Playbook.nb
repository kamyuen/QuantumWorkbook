  3.915482679110826*^9}},
 CellID->1826763661,ExpressionUUID->"3547f6e0-ff3d-42bd-86ff-97f95a686fcc"],

Cell[TextData[{
 "\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "\[RightTeeArrow]", 
    RowBox[{"F", "(", "k", ")"}]}], TraditionalForm]],ExpressionUUID->
  "18525d68-8085-4513-8b5d-6b7a95fdcb03"]
}], "Text",
 CellChangeTimes->{{3.914902249728258*^9, 3.914902276107258*^9}, {
  3.915482376864409*^9, 3.915482393297404*^9}, {3.915482678470313*^9, 
  3.915482681239137*^9}},
 CellID->1191486009,ExpressionUUID->"19d81472-ab76-4c53-ba8a-7b807459780e"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "(", "k", ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"2", "\[Pi]"}]]], 
     RowBox[{"\[Integral]", 
      RowBox[{"dx", " ", 
       RowBox[{"f", "(", "x", ")"}], "\[VeryThinSpace]", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "x", "\[VeryThinSpace]", 
         "k"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "5ae9d20a-e36e-48b3-a29a-2b4becfa2317"]
}], "Text",
 CellChangeTimes->{{3.914901846297113*^9, 3.91490192806854*^9}, {
   3.914901988203071*^9, 3.914902022303907*^9}, {3.914902080441259*^9, 
   3.9149020855175962`*^9}, 3.914902277481697*^9},
 CellID->820780351,ExpressionUUID->"693fa1f3-6011-4d61-8d87-2d3bbb323c7e"],

Cell["Discrete Fourier transform (DFT)", "Item",
 CellChangeTimes->{{3.914902257394033*^9, 3.914902264094943*^9}, {
  3.915482399689146*^9, 3.915482453410646*^9}, {3.915482684607763*^9, 
  3.915482685352505*^9}},
 CellID->603292624,ExpressionUUID->"77f803f2-932a-4abc-ab1e-99509df97671"],

Cell[TextData[{
 "\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["f", "x"], "|", "x"}], "=", "0"}], ",", "1", ",", "2", 
      ",", "\[Ellipsis]", ",", 
      RowBox[{"N", "-", "1"}]}], "}"}], "\[RightArrow]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["F", "k"], "|", "k"}], "=", "0"}], ",", "1", ",", "2", 
       ",", "...", ",", "N"}], "}"}], "-", "1"}]}], TraditionalForm]],
  ExpressionUUID->"d24f13e0-854e-48eb-bb8e-94529927ad12"]
}], "Text",
 CellChangeTimes->{{3.914902257394033*^9, 3.914902264094943*^9}, {
  3.915482399689146*^9, 3.915482453410646*^9}, {3.915482684607763*^9, 
  3.915482693285243*^9}},
 CellID->1117138768,ExpressionUUID->"dde49650-6c72-407e-804a-b80f71d36509"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "k"], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{"N", "-", "1"}]], 
     SubscriptBox["f", "x"], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
       RowBox[{"x", "\[VeryThinSpace]", "(", 
        RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", 
         RowBox[{"k", "/", "N"}]}], ")"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"aee025ec-78ab-4765-b6a0-6b492c3278b6"]
}], "Text",
 CellChangeTimes->{{3.914901999848721*^9, 3.914902002977979*^9}, {
   3.914902059063909*^9, 3.914902192909396*^9}, 3.914902279012776*^9, {
