  3.881614727000866*^9, {3.915423225448798*^9, 3.915423227037856*^9}, {
   3.915423370423993*^9, 3.915423370559547*^9}, {3.91542369657277*^9, 
   3.915423698538118*^9}, {3.915423758799216*^9, 3.915423763978285*^9}},
