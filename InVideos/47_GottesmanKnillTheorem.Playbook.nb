     },
     GridBoxAlignment->{
      "Columns" -> {Right, Center, {Center}}, "Rows" -> {{Baseline}}}]}], 
   TraditionalForm]],ExpressionUUID->"1961ef3e-de9a-4426-992a-ce22e4d38bc0"]
