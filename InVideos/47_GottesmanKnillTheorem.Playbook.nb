  RowBox[{"[", 
   RowBox[{";;", 
    RowBox[{"UpTo", "[", "10", "]"}]}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.9154233195031753`*^9, 3.9154233318633423`*^9}, {
