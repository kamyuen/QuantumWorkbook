Cell[TextData[{
 StyleBox["Convention",
  FontWeight->"Bold"],
 ": ",
 StyleBox["Q3", "Input"],
 " refers to the eigenvalues \[PlusMinus]1 of a Pauli string by binary digits \
0 and 1, respectively;"
}], "Item",
 CellChangeTimes->{{3.894766582769764*^9, 3.89476658417245*^9}, 
   3.91565537975581*^9, 3.915655437608489*^9},
