Cell[TextData[{
 "We want to measure ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Z", "1"], 
    SubscriptBox["Z", "2"], 
    SubscriptBox["\[Ellipsis]Z", "n"]}], TraditionalForm]],ExpressionUUID->
  "b2a4c44f-36cd-4002-83ef-558bf05fe270"],
 ", where ",
