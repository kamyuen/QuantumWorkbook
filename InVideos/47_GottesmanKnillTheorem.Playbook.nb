 CellLabel->"In[26]:=",
 CellID->1497351148,ExpressionUUID->"c8aa6c31-ed9c-4b20-90ff-94a438d17c74"],

Cell[TextData[{
 "We fist note that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Z", "1"], 
     SubscriptBox["Z", "2"], 
     SubscriptBox["\[Ellipsis]Z", "n"]}], "\[LeftRightArrow]", 
    RowBox[{
     SubscriptBox["x", "1"], "\[CirclePlus]", 
     SubscriptBox["x", "2"], "\[CirclePlus]", "\[Ellipsis]", "\[CirclePlus]", 
     
     SubscriptBox["x", "n"]}]}], TraditionalForm]],ExpressionUUID->
  "5ec83b01-7a39-41ea-a4c5-94dc85c93290"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "k"], "\[Element]", 
    RowBox[{"{", 
     RowBox[{"0", ",", "1"}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "3c229415-4a42-4b5e-9b41-48268247961a"],
 "."
}], "Text",
 CellID->215502103,ExpressionUUID->"bb69087f-f3df-4b49-b6a7-6f269c38181b"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$n", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$N", "=", 
   RowBox[{"Power", "[", 
    RowBox[{"2", ",", "$n"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"kk", "=", 
   RowBox[{"Range", "[", "$n", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"SS", "=", 
  RowBox[{"S", "[", 
   RowBox[{
    RowBox[{"Range", "@", "$n"}], ",", "$"}], "]"}]}]}], "Input",
 CellChangeTimes->{
  3.894766653364205*^9, {3.915669785907158*^9, 3.915669786657953*^9}},
 CellLabel->"In[66]:=",
 CellID->749608997,ExpressionUUID->"452c6610-7021-4355-a3e6-4a222781fe2b"],

Cell[BoxData[
 RowBox[{"qc", "=", 
  RowBox[{"QuantumCircuit", "[", 
   RowBox[{
    RowBox[{"Ket", "[", 
     RowBox[{"{", "T", "}"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Sequence", "@@", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"CNOT", "[", 
         RowBox[{"#", ",", "T"}], "]"}], "&"}], ",", "SS"}], "]"}]}], ",", 
    "\"\<Spacer\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"Measurement", "[", 
     RowBox[{"T", "[", "3", "]"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<Invisible\>\"", "->", 
     RowBox[{"S", "[", 
      RowBox[{"$n", "+", 
       RowBox[{"1", "/", "2"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.885926284033842*^9, 3.885926285177915*^9}, {
  3.915669794500463*^9, 3.915669801885593*^9}},
 CellLabel->"In[73]:=",
 CellID->1412008253,ExpressionUUID->"5ba019d1-09db-4252-9436-14156886f818"],

Cell["Take a superposition of all computational basis states.", "Text",
 CellID->1304792321,ExpressionUUID->"63e08acf-c5ef-4187-b387-0e5430d13217"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"in", "=", 
   RowBox[{"Total", "@", 
    RowBox[{"Basis", "[", "SS", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"in", "//", "SimpleForm"}]}], "Input",
 CellChangeTimes->{{3.915669817164833*^9, 3.91566982098828*^9}},
 CellLabel->"In[78]:=",
 CellID->1123804818,ExpressionUUID->"7e41938d-6090-4913-a965-da4101d42db2"],

