Cell[TextData[{
 "As another example, we consider a interpolation between the completely \
random state I/2 and a pure state (I+ ",
 StyleBox["a",
  FontWeight->"Bold"],
 "\[CenterDot]",
 StyleBox["S",
  FontWeight->"Bold"],
 ")/2 with \[DoubleVerticalBar]",
 StyleBox["a",
  FontWeight->"Bold"],
 "\[DoubleVerticalBar]=1."
}], "MathCaption",
 CellChangeTimes->{{3.839267784720559*^9, 
  3.8392679336171007`*^9}},ExpressionUUID->"f03e8929-4743-4a95-9a29-\
0297b6a18b90"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"rho", "=", 
  RowBox[{
   RowBox[{"1", "/", "2"}], "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "-", "p"}], ")"}], 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], 
         RowBox[{"Cos", "[", "\[Phi]", "]"}]}], ",", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], 
         RowBox[{"Sin", "[", "\[Phi]", "]"}]}], ",", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "}"}], ".", 
      RowBox[{"S", "[", "All", "]"}]}], "/", 
     "2"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Tr", "[", 
   RowBox[{"Matrix", "[", "rho", "]"}], "]"}], "//", "Simplify"}]}], "Input",
 CellChangeTimes->{{3.8392679434857883`*^9, 3.8392680788594427`*^9}, {
  3.839268244576501*^9, 3.839268257657921*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"0ebb8761-d14f-4a7b-ac09-a5048e995ec4"],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "2"], "+", 
  RowBox[{
   FractionBox["1", "2"], " ", 
   RowBox[{"(", 
    RowBox[{"1", "-", "p"}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"Cos", "[", "\[Theta]", "]"}], " ", 
      TemplateBox[{
        TemplateBox[{"S"}, "RowDefault"], 
        TemplateBox[{"\"z\""}, "RowDefault"]},
       "Superscript"]}], "+", 
     RowBox[{
      RowBox[{"Cos", "[", "\[Phi]", "]"}], " ", 
      TemplateBox[{
        TemplateBox[{"S"}, "RowDefault"], 
        TemplateBox[{"\"x\""}, "RowDefault"]},
       "Superscript"], " ", 
      RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "+", 
     RowBox[{
      TemplateBox[{
        TemplateBox[{"S"}, "RowDefault"], 
        TemplateBox[{"\"y\""}, "RowDefault"]},
       "Superscript"], " ", 
      RowBox[{"Sin", "[", "\[Theta]", "]"}], " ", 
      RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], ")"}]}]}]], "Output",
 CellChangeTimes->{{3.8392679956896343`*^9, 3.839268083244938*^9}, {
   3.839268251795285*^9, 3.839268264296055*^9}, 3.8393691952821407`*^9},
 CellLabel->"Out[9]=",ExpressionUUID->"414d7f5b-572d-454d-83d9-cae40b0ca4fe"],

