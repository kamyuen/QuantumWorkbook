Cell[TextData[{
 "We consider a simple example. The density operator describes an ensemble of \
two pure states ",
 Cell[BoxData[
  TemplateBox[{"0"},
   "Ket"]],ExpressionUUID->"39663315-b026-4773-b535-65b3986fd6ea"],
 " and ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{"+"},
    "Ket"], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      TemplateBox[{"0"},
       "Ket"], "+", 
      TemplateBox[{"1"},
       "Ket"]}], ")"}], "/", 
    SqrtBox["2"]}]}]],ExpressionUUID->
  "1ddb6792-7e3c-4881-b94a-d0dfeb8a4001"],
 "."
}], "MathCaption",
 CellChangeTimes->{{3.8392673037758303`*^9, 3.839267385558619*^9}, {
  3.8392676759952087`*^9, 
  3.839267689437354*^9}},ExpressionUUID->"725b0ac9-bf28-4960-ab21-\
c7311c1327ef"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"vec", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Ket", "[", "]"}], "+", 
      RowBox[{"Ket", "[", 
       RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
    RowBox[{"Sqrt", "[", "2", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"rho", "=", 
  RowBox[{
   RowBox[{"p", "*", 
    RowBox[{"Dyad", "[", 
     RowBox[{
      RowBox[{"Ket", "[", "]"}], ",", 
      RowBox[{"Ket", "[", "]"}]}], "]"}]}], "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "-", "p"}], ")"}], "*", 
    RowBox[{"Dyad", "[", 
     RowBox[{"vec", ",", "vec"}], "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Matrix", "[", "rho", "]"}], "//", "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.8392673923570023`*^9, 3.839267489503368*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"3ab5d6f6-cd44-454e-a2d3-387e77580c44"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "-", "p"}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      TemplateBox[{
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["0", "S"]}, "RowDefault"], DisplayForm]}, "Ket"], 
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["0", "S"]}, "RowDefault"], DisplayForm]}, "Bra"]},
       "RowDefault"], "2"], "+", 
     FractionBox[
      TemplateBox[{
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["0", "S"]}, "RowDefault"], DisplayForm]}, "Ket"], 
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["1", "S"]}, "RowDefault"], DisplayForm]}, "Bra"]},
       "RowDefault"], "2"], "+", 
     FractionBox[
      TemplateBox[{
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["1", "S"]}, "RowDefault"], DisplayForm]}, "Ket"], 
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["0", "S"]}, "RowDefault"], DisplayForm]}, "Bra"]},
       "RowDefault"], "2"], "+", 
     FractionBox[
      TemplateBox[{
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["1", "S"]}, "RowDefault"], DisplayForm]}, "Ket"], 
        TemplateBox[{
          TagBox[
           TemplateBox[{
             SubscriptBox["1", "S"]}, "RowDefault"], DisplayForm]}, "Bra"]},
       "RowDefault"], "2"]}], ")"}]}], "+", 
  RowBox[{"p", " ", 
   TagBox[
    RowBox[{
     TagBox[
      TemplateBox[{"\"\[SpaceIndicator]\""},
       "Ket"],
      DisplayForm], 
     TagBox[
      TemplateBox[{"\"\[SpaceIndicator]\""},
       "Bra"],
      DisplayForm]}],
    DisplayForm]}]}]], "Output",
