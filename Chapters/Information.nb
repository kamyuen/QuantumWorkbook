 RowBox[{
  RowBox[{"$n", "=", "2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"cc", "=", 
   RowBox[{"Range", "[", "$n", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tt", "=", 
   RowBox[{"$n", "+", "cc"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.840674469495285*^9, 3.840674500869276*^9}, {
  3.840674561978297*^9, 3.840674572384177*^9}, {3.840674647422141*^9, 
