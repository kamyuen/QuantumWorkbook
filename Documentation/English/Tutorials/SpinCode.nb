Cell[CellGroupData[{

Cell["Spin Operators", "Subsection",
 CellChangeTimes->{{3.8960720416065397`*^9, 3.8960720435150833`*^9}},
 CellID->931913261,ExpressionUUID->"02df8062-35f7-48a4-9728-84be08f41a0c"],

Cell[CellGroupData[{

Cell["These are the spin operators.", "MathCaption",
 CellChangeTimes->{{3.8960612699165792`*^9, 3.896061275615288*^9}},
 CellID->1067940056,ExpressionUUID->"1ac33645-83f1-4e04-b7a3-9598a152d285"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"S", "[", "All", "]"}]], "Input",
 CellChangeTimes->{{3.896056763438684*^9, 3.8960567692092457`*^9}, {
  3.8960612403821774`*^9, 3.896061267818755*^9}},
 CellLabel->"In[103]:=",
 CellID->200199566,ExpressionUUID->"7e0c5f8c-153e-42e6-bfe2-ac5c52833952"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   InterpretationBox[
    TemplateBox[{"S", 
      TemplateBox[{"\[ThinSpace]", "\"\[ThinSpace]\"", "\"X\""}, 
       "RowWithSeparators"]},
     "Superscript"],
    $CellContext`S[1]], ",", 
   InterpretationBox[
    TemplateBox[{"S", 
      TemplateBox[{"\[ThinSpace]", "\"\[ThinSpace]\"", "\"Y\""}, 
