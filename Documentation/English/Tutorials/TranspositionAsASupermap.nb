Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", 
     OverscriptBox["\[Rho]", "^"], ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "\[VeryThinSpace]", "l"}]], 
     SubscriptBox[
      OverscriptBox["F", "^"], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     OverscriptBox["\[Rho]", "^"], 
     SubsuperscriptBox[
      OverscriptBox["F", "^"], 
      RowBox[{"k", "\[VeryThinSpace]", "l"}], "\[Dagger]"], 
     SubscriptBox["C", 
      RowBox[{
       RowBox[{"i", "\[VeryThinSpace]", "j"}], ";", 
       RowBox[{"k", "\[VeryThinSpace]", "l"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"707ef30e-1723-4947-841a-6c1fee1de93f"],
 " ,"
}], "Text",
 CellID->621426667,ExpressionUUID->"d3107efc-face-4036-959a-ce604784f14d"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["F", "^"], 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["v", "i"]},
      "Ket"], 
     TemplateBox[{
       SubscriptBox["v", "j"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "517b21fe-7f81-49da-9004-406b4e69cc63"],
 ". An equivalent definition is"
}], "Text",
 CellID->1105016908,ExpressionUUID->"efc9cfe6-ce89-41b4-824a-f6261b2d96b5"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["v", "k"]},
       "Ket"], 
      TemplateBox[{
        SubscriptBox["v", "l"]},
       "Bra"]}], ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     TemplateBox[{
       SubscriptBox["v", "i"]},
      "Ket"], 
     SubscriptBox["C", 
      RowBox[{
       RowBox[{"i", "\[VeryThinSpace]", "k"}], ";", 
       RowBox[{"j", "\[VeryThinSpace]", "l"}]}]], 
     TemplateBox[{
       SubscriptBox["v", "j"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "bdafc302-185c-44a3-b57e-869a23155599"]
}], "Text",
 CellID->580593500,ExpressionUUID->"a128e5d3-34db-426a-a951-938f13429790"],

