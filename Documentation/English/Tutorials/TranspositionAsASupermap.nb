Cell[TextData[{
 "Recall the defining equation of the Choi matrix ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]],ExpressionUUID->
  "1672a016-fe25-4d35-b684-485e424baa05"],
 " of a supermap ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalF]", TraditionalForm]],ExpressionUUID->
  "846928c7-c3ff-479f-984d-c1119a2ce204"],
 ","
}], "Text",
 CellID->907737565,ExpressionUUID->"3b819bf2-58d8-498f-94c5-e0a7e6d3b26c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", 
     OverscriptBox["\[Rho]", "^"], ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "\[VeryThinSpace]", "l"}]], 
     SubscriptBox[
      OverscriptBox["F", "^"], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     OverscriptBox["\[Rho]", "^"], 
     SubsuperscriptBox[
      OverscriptBox["F", "^"], 
      RowBox[{"k", "\[VeryThinSpace]", "l"}], "\[Dagger]"], 
     SubscriptBox["C", 
      RowBox[{
       RowBox[{"i", "\[VeryThinSpace]", "j"}], ";", 
       RowBox[{"k", "\[VeryThinSpace]", "l"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"707ef30e-1723-4947-841a-6c1fee1de93f"],
 " ,"
}], "Text",
 CellID->621426667,ExpressionUUID->"d3107efc-face-4036-959a-ce604784f14d"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["F", "^"], 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["v", "i"]},
      "Ket"], 
     TemplateBox[{
       SubscriptBox["v", "j"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "517b21fe-7f81-49da-9004-406b4e69cc63"],
 ". An equivalent definition is"
}], "Text",
 CellID->1105016908,ExpressionUUID->"efc9cfe6-ce89-41b4-824a-f6261b2d96b5"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["v", "k"]},
       "Ket"], 
      TemplateBox[{
        SubscriptBox["v", "l"]},
       "Bra"]}], ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     TemplateBox[{
       SubscriptBox["v", "i"]},
      "Ket"], 
     SubscriptBox["C", 
      RowBox[{
       RowBox[{"i", "\[VeryThinSpace]", "k"}], ";", 
       RowBox[{"j", "\[VeryThinSpace]", "l"}]}]], 
     TemplateBox[{
       SubscriptBox["v", "j"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "bdafc302-185c-44a3-b57e-869a23155599"]
}], "Text",
 CellID->580593500,ExpressionUUID->"a128e5d3-34db-426a-a951-938f13429790"],

Cell[TextData[{
 "with respect to a given orthonormal basis ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["v", "k"]},
        "Ket"], "|", "k"}], "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
     "d"}], "}"}], TraditionalForm]],ExpressionUUID->
  "b6cb98bd-40b3-4576-b2e0-d3ae4c360e7f"],
 "."
}], "Text",
 CellID->2016746179,ExpressionUUID->"55219ad2-8fc8-4544-96e3-62d4c81aa13a"],

Cell["The Choi matrix of the transposition supermap is given by", "Text",
 CellID->1718629852,ExpressionUUID->"3bfa3655-b8cc-4f7d-98d8-377e51d051b9"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", 
     RowBox[{
      RowBox[{"i", "\[VeryThinSpace]", "j"}], ";", 
      RowBox[{"k", "\[VeryThinSpace]", "l"}]}]], "=", 
    RowBox[{
     SubscriptBox["\[Delta]", 
      RowBox[{"j", "\[VeryThinSpace]", "k"}]], 
