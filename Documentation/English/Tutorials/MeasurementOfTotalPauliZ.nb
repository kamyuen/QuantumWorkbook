Cell[TextData[{
 "This is a short-cut function:  ",
 Cell[BoxData[
  RowBox[{"addBitValue", "[", 
   StyleBox["p", "TI"], "]"}]], "InlineFormula",ExpressionUUID->
  "b8a1e216-99a1-4d26-933d-cd821e8cc14b"],
 " generates the quantum circuit elements to add the bit value of qubit ",
 Cell[BoxData[
  StyleBox["p", "TI"]], "InlineFormula",ExpressionUUID->
  "f24b9bdb-41b6-457b-a488-70d210d8f919"],
 "."
}], "MathCaption",
