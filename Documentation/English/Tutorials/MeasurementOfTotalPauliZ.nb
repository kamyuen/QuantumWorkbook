  3.882062438406137*^9, 3.882062473906468*^9}, {3.8820626108923283`*^9, 
  3.88206262794586*^9}},
 CellID->1879465639,ExpressionUUID->"7e21c874-8a85-4a1b-88c9-d556d201984f"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "addBitValue", "]"}], ";", 
  RowBox[{
   RowBox[{"addBitValue", "[", "]"}], ":=", 
   RowBox[{"Sequence", "@@", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"addBitValue", "[", "p", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"p", ",", "$n"}], "}"}]}], 
     "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"addBitValue", "[", "p_", "]"}], ":=", 
  RowBox[{"Sequence", "@@", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"logp", "=", 
       RowBox[{"Ceiling", "@", 
        RowBox[{"Log", "[", 
         RowBox[{"2", ",", 
          RowBox[{"p", "+", "1"}]}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"CNOT", "[", 
        RowBox[{
         RowBox[{"Prepend", "[", 
          RowBox[{
           RowBox[{"T", "@", 
            RowBox[{"Range", "[", 
             RowBox[{"k", "-", "1"}], "]"}]}], ",", 
           RowBox[{"S", "[", "p", "]"}]}], "]"}], ",", 
         RowBox[{"T", "[", "k", "]"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "logp", ",", "1", ",", 
         RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 CellChangeTimes->{{3.87690333931577*^9, 3.8769033445290318`*^9}, {
  3.876903387530692*^9, 3.876903400218215*^9}, {3.8820516624332647`*^9, 
