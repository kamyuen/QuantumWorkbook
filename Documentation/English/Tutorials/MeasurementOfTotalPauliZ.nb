 RowBox[{
  RowBox[{"kk", "=", 
   RowBox[{"Range", "[", "$n", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ln", "=", 
   RowBox[{"Ceiling", "@", 
    RowBox[{"Log", "[", 
     RowBox[{"2", ",", 
      RowBox[{"$n", "+", "1"}]}], "]"}]}]}], ";"}]}], "Input",
