  RowBox[{"SS", "=", 
   RowBox[{"S", "[", 
    RowBox[{
     RowBox[{"Range", "@", "$n"}], ",", "None"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TT", "=", 
   RowBox[{"T", "[", 
    RowBox[{
     RowBox[{"Range", "@", "$l"}], ",", "None"}], "]"}]}], ";"}]}], "Input",
