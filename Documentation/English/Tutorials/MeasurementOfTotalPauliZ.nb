}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "We want to measure ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["S", "tot", "z"], ":=", 
    RowBox[{
     SubsuperscriptBox["S", "1", "z"], "+", 
     SubsuperscriptBox["S", "2", "z"], "+", "\[Ellipsis]", "+", 
     SubsuperscriptBox["S", "n", "z"]}]}], TraditionalForm]],ExpressionUUID->
  "fe6c5dd3-cc64-4940-942e-ec7c80f81461"],
 ". The auxiliary register ",
 StyleBox["T",
  FontSlant->"Italic"],
 " is used to store the result."
}], "Text",
 CellChangeTimes->{{3.876903015677902*^9, 3.876903096630252*^9}, {
  3.8769031486702137`*^9, 3.876903220285674*^9}},
