Cell[BoxData[
 RowBox[{
  RowBox[{"prb", "[", 
   RowBox[{"\[Phi]_", ",", "m_"}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"p", "=", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"\[Phi]", "/", "2"}], "]"}], "^", "2"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Binomial", "[", 
      RowBox[{"$n", ",", "m"}], "]"}], "*", 
