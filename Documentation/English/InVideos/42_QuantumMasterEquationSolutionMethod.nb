Cell[TextData[{
 "Integrating quantum master equation gets quickly intractable as the system \
increases in size. For example, for a system of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "635504fd-1864-4711-82e6-8c25105af39f"],
 " qubits, one has to deal with an order of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["4", "n"], "\[Times]", 
    SuperscriptBox["4", "n"]}], TraditionalForm]],ExpressionUUID->
  "6083daaf-51df-4790-87ea-f51e6cb566fd"],
 "matrix. The required memory is simply too large for most available \
computers."
}], "Text",
 CellChangeTimes->{{3.8944886568806868`*^9, 3.89448866862607*^9}},
 CellID->2086703375,ExpressionUUID->"cbea9625-6aeb-4834-ba10-234b0fc90d38"],

Cell[TextData[{
 "For a short elapse ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"6ed80e0d-f952-44ee-88ea-8c2583d32767"],
 " of time from ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]],ExpressionUUID->
  "7cc23a9d-2757-40d9-8789-acf39387f369"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "+", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], TraditionalForm]],
  ExpressionUUID->"73a12b4a-b943-46d9-ac5c-9ddf363d4a83"],
 ", up to the first order in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"adc4bcd6-e9ee-4504-839f-b0ba3a58485b"],
 ", the density operator evolves as"
}], "Text",
 CellChangeTimes->{3.894488671533061*^9},
 CellID->668184468,ExpressionUUID->"0dbaeede-68c1-4d4f-aad3-aedd41121f60"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["U", "non"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      RowBox[{
       SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"5263724f-c97d-47df-b22b-2ef7d796b81b"],
 " ,"
}], "Text",
