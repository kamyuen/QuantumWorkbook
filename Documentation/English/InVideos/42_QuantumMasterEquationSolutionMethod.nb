Cell[BoxData[{
 RowBox[{"Let", "[", 
  RowBox[{"Real", ",", "\[CapitalGamma]"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"opL", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<+\>\"", "]"}], "]"}], " ", 
     RowBox[{"S", "[", "4", "]"}]}], ",", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<-\>\"", "]"}], "]"}], 
     RowBox[{"S", "[", "5", "]"}]}]}], "}"}]}]}], "Input",
 CellLabel->"In[3]:=",
 CellID->890120871,ExpressionUUID->"3c969162-c90f-41a9-9ea3-94fa0857228d"],

Cell["Take an initial state.", "Text",
 CellID->1184140969,ExpressionUUID->"f6a8962b-ed27-4865-8da1-39117f901675"],

Cell[BoxData[
 RowBox[{"in", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Ket", "[", "]"}], "+", 
      RowBox[{"Ket", "[", 
       RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
    RowBox[{"Sqrt", "[", "2", "]"}]}], "//", "KetRegulate"}]}]], "Input",
 CellLabel->"In[5]:=",
