Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "A", ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]], 
     SubscriptBox["F", "\[Mu]"], "A", " ", 
