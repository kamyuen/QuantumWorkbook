Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "non"], "(", "t", ")"}], TraditionalForm]],
  ExpressionUUID->"7becf6d7-4fdd-4348-8b33-3bb57b180817"],
 " is the non-unitary time-evolution operator associated with the effective \
non-Hermitian Hamiltonian; that is,"
}], "Text",
 CellID->2131030150,ExpressionUUID->"aa418262-cc6c-4553-b63c-a68f2916b31d"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "non"], "(", "t", ")"}], ":=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", "t", 
      "\[VeryThinSpace]", 
      SubscriptBox["H", "non"]}]]}], TraditionalForm]],ExpressionUUID->
  "a779467d-2637-40b8-945a-79229e4197f3"],
 " ."
}], "Text",
 CellFrameLabels->{{None, 
    Cell[
