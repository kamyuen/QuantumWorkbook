Cell[BoxData[{
 RowBox[{"Let", "[", 
  RowBox[{"Real", ",", "\[CapitalGamma]"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"opL", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<+\>\"", "]"}], "]"}], " ", 
     RowBox[{"S", "[", "4", "]"}]}], ",", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"\[CapitalGamma]", "[", "\"\<-\>\"", "]"}], "]"}], 
     RowBox[{"S", "[", "5", "]"}]}]}], "}"}]}]}], "Input",
 CellLabel->"In[3]:=",
 CellID->890120871,ExpressionUUID->"685ead85-2e28-4801-8a0a-97cbb46e6cbe"],

Cell["Take an initial state.", "Text",
 CellID->1184140969,ExpressionUUID->"b0c2b797-2db2-4ab1-96b8-1280bd8a326a"],

Cell[BoxData[
 RowBox[{"in", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Ket", "[", "]"}], "+", 
      RowBox[{"Ket", "[", 
       RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
    RowBox[{"Sqrt", "[", "2", "]"}]}], "//", "KetRegulate"}]}]], "Input",
 CellLabel->"In[5]:=",
 CellID->1126309234,ExpressionUUID->"ee817e38-0555-40c5-8ee3-8880580c45fb"],

