Cell[TextData[{
 "Integrating quantum master equation gets quickly intractable as the system \
increases in size. For example, for a system of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "170ca688-87ea-46b7-b437-a823685af1f5"],
 " qubits, one has to deal with an order of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["4", "n"], "\[Times]", 
    SuperscriptBox["4", "n"]}], TraditionalForm]],ExpressionUUID->
  "606dbeb4-a5bd-4f7d-82bd-2cca5dd1de92"],
 "matrix. The required memory is simply too large for most available \
computers."
}], "Text",
 CellChangeTimes->{{3.8944886568806868`*^9, 3.89448866862607*^9}},
 CellID->2086703375,ExpressionUUID->"8ef99763-dd99-4920-985c-1bf125ede1c7"],

Cell[TextData[{
 "For a short elapse ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"cd5e197e-3e9c-4de3-b2de-25a2e887a3c9"],
 " of time from ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]],ExpressionUUID->
  "0c71c2af-be02-4ed9-99d1-86dfcb2a4b8a"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "+", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], TraditionalForm]],
  ExpressionUUID->"9f9c4433-6c20-49a6-9a0a-357cfa0714a6"],
 ", up to the first order in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"f034de04-89a1-4612-8042-19255d1de5cc"],
 ", the density operator evolves as"
}], "Text",
 CellChangeTimes->{3.894488671533061*^9},
 CellID->668184468,ExpressionUUID->"2b51ae38-5b83-4f9b-ad8c-8a0c2b47416e"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["U", "non"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      RowBox[{
       SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"b0f97849-1d02-46f2-8473-f663e4eceb17"],
 " ,"
}], "Text",
 CellFrameLabels->{{None, 
    Cell[
     TextData[{"(", 
       CounterBox["DisplayFormulaNumbered"], ")"}], 
     "DisplayFormulaEquationNumber"]}, {None, None}},
 CounterIncrements->"DisplayFormulaNumbered",
