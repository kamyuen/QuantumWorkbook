 CellID->776887413,ExpressionUUID->"dae42b29-4e08-4bde-9f67-a0a6e35e974a"],

Cell[TextData[{
 "For all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", ",", 
    RowBox[{"y", "\[Element]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"ad416b2c-35f2-4e5f-bb07-d5eefb0bda5a"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{"f", 
     RowBox[{"(", "y", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "94213c3f-56ad-48dd-8c81-49b90e80122e"],
 " if and only if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"x", "\[CirclePlus]", "s"}]}], TraditionalForm]],ExpressionUUID->
  "14b82f8f-3da4-4b24-ade5-641a627c0f77"],
 ". Note that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "526e2f47-1d57-4a39-841b-f00ecf02929d"],
 " is either one-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "4aa0f7a9-d8b2-4839-9b2c-c89b09164ee4"],
 ") or two-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "1bfbd3a8-0182-41de-8857-82d53a272764"],
 "). "
}], "ItemNumbered",
 CellChangeTimes->{{3.852070435901077*^9, 3.852070716088048*^9}, 
   3.855686665351173*^9, 3.893985211575436*^9, {3.8992008623298483`*^9, 
