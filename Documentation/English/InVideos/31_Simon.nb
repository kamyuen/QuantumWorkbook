Cell[TextData[{
 "The quantum circuit in Figure 1 summarizes Simon\[CloseCurlyQuote]s \
algorithm. The first ",
 Cell[BoxData[
  ButtonBox["Hadamard",
   BaseStyle->"Link",
   ButtonData->"paclet:Q3/ref/Hadamard"]], "InlineFormula",ExpressionUUID->
  "d472cea8-7fad-4100-a322-5ab9148c21c8"],
 " gate on the native register transforms the input state of the whole system \
as"
}], "Text",
 CellChangeTimes->{{3.8520708004625883`*^9, 3.852070828216937*^9}, 
   3.852070878080828*^9, {3.8520712020295568`*^9, 3.852071210375574*^9}, {
   3.892249951597343*^9, 3.892249985988494*^9}},
 CellID->435207388,ExpressionUUID->"570936a9-b891-4568-9ea6-f37810fdd9f2"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"0"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"0"},
      "Ket"]}], 
    OverscriptBox["\[RightArrow]", 
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]]], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     RowBox[{
      TemplateBox[{"x"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"0"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "de2e182e-8fec-414b-a4fa-90f697d2a77b"],
 "."
}], "Text",
 CellChangeTimes->{{3.852071295311932*^9, 3.85207131160734*^9}, {
  3.852071349026947*^9, 3.8520714143334284`*^9}, {3.8939859761943502`*^9, 
  3.8939859761944*^9}},
 CellID->1976242829,ExpressionUUID->"c89d906c-ec59-4569-851b-e9bb893bba43"],

