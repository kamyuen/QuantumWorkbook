    RowBox[{"I", "*", 
     RowBox[{"Ket", "[", 
      RowBox[{
       RowBox[{"S", "[", "1", "]"}], "\[Rule]", "1"}], "]"}]}]}], " ", "//", 
   "KetRegulate"}]}]], "Input",
