    RowBox[{"S", "@", 
     RowBox[{"{", 
      RowBox[{"1", ",", "2", ",", "3"}], "}"}]}], "->", 
    RowBox[{"{", 
     RowBox[{"0", ",", "1", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.885581687904107*^9, 3.885581733164291*^9}},
