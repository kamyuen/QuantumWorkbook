Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      SubscriptBox["X", "y"]},
     "Ket"], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["y", "1"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["y", "2"]},
      "Ket"], "\[CircleTimes]", "\[Ellipsis]", "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["y", "n"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "407f5f81-a9dd-4d80-aeba-a1e6e8670488"]
}], "Text",
