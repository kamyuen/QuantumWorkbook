Cell[TextData[{
 StyleBox["Theorem",
  FontWeight->"Bold"],
 ": Any Pauli string has eigenvalues \[PlusMinus]1."
}], "Text",
 CellChangeTimes->{{3.915422012215411*^9, 3.915422057431589*^9}, {
  3.91542462769077*^9, 3.915424631828663*^9}},
