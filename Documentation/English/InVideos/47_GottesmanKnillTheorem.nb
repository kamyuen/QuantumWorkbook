Cell[CellGroupData[{

Cell["Clifford Operators", "Section",
 CellChangeTimes->{{3.9145505215338907`*^9, 3.914550524555667*^9}},
 CellID->1230124648,ExpressionUUID->"73b13e1b-8b40-427e-a574-ff87bdfc7b40"],

Cell[TextData[{
 StyleBox["Definition (Pauli string)",
  FontWeight->"Bold"],
 ": A tensor product of single-qubit Pauli operators (such as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Z", "\[CircleTimes]", "Z", "\[CircleTimes]", "X"}], 
    "\[VeryThinSpace]", "\[Ellipsis]"}], TraditionalForm]],ExpressionUUID->
  "70cdf0d1-fbc3-44be-8742-6b94ce2d3930"],
 ") is called a ",
 StyleBox["Pauli string",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.915424522812743*^9, 3.915424565602877*^9}, {
  3.915651993669158*^9, 3.915651996905614*^9}},
 CellID->2042336787,ExpressionUUID->"fa328e7c-ed29-4ea6-95fe-b66a919033bc"],

Cell[TextData[{
 StyleBox["Definition (Clifford operator)",
  FontWeight->"Bold"],
 ": A unitary operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "073f69cb-e10d-4cb5-92ad-e8363731ea2a"],
 " is called a ",
 StyleBox["Clifford operator",
  FontSlant->"Italic"],
 " if it transforms a Pauli string to another Pauli string."
}], "Text",
 CellChangeTimes->{{3.915424195597692*^9, 3.915424245494551*^9}, {
  3.915652000215826*^9, 3.9156520066375523`*^9}},
 CellID->1488032392,ExpressionUUID->"dbca608c-37c0-43e7-9d42-93cfa74dc08c"],

Cell[TextData[{
 StyleBox["Theorem",
  FontWeight->"Bold"],
 ": Any Clifford operator may be generated by combining ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "b4990f3e-6284-4606-85c6-18481bed27a2"],
 ", ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "e57e472b-2fe6-406c-b82c-5b4e27004ec2"],
 ", and CNOT."
}], "Text",
 CellChangeTimes->{{3.915424267222658*^9, 3.915424333899441*^9}, {
  3.915652011672028*^9, 3.915652015292905*^9}},
 CellID->2036002162,ExpressionUUID->"e4c0ae98-9041-4005-8ece-496b8d1a9cf4"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["2"]], 
     RowBox[{"(", GridBox[{
        {"1", "1"},
        {"1", 
         RowBox[{"-", "1"}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "c538f7d5-90b9-477d-93e3-9fd6087f2370"],
 ",     ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", ":=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"1", "0"},
        {"0", "\[ImaginaryI]"}
       }], ")"}], "=", 
     SqrtBox["Z"]}]}], TraditionalForm]],ExpressionUUID->
  "b5c0ae99-7867-458b-83bc-3b5168af3fae"],
 " ."
}], "Text",
 CellChangeTimes->{{3.915424337096229*^9, 3.9154244038991537`*^9}, {
  3.915651869437584*^9, 3.9156518722218122`*^9}},
 CellID->1659561479,ExpressionUUID->"cc656340-860d-4613-93b5-99bc59a6d03e"],

Cell[TextData[{
 "Note that the so-called ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "cdc082a3-d2d0-4fad-b633-fffe47a9789d"],
 "-gate (",
 StyleBox["octant",
  FontSlant->"Italic"],
 "-phase gate) is ",
 StyleBox["not",
  FontSlant->"Italic"],
 " a Clifford operator;"
}], "Text",
 CellChangeTimes->{{3.915651904171506*^9, 3.9156519605642967`*^9}, {
  3.915652134780789*^9, 3.915652143023656*^9}, {3.915652194439003*^9, 
  3.915652209238888*^9}},
 CellID->2027699292,ExpressionUUID->"44e18f7a-dbd7-47ec-99f7-3efc006d1d8c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"T", ":=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"1", "0"},
        {"0", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
           RowBox[{"\[Pi]", "/", "4"}]}]]}
       }], ")"}], "=", 
     SqrtBox["S"]}]}], TraditionalForm]],ExpressionUUID->
  "2eb59eaf-e8f3-4371-be05-509fc2a222cd"]
}], "Text",
 CellChangeTimes->{{3.915652144468548*^9, 3.915652176614195*^9}},
 CellID->715511707,ExpressionUUID->"5b11d848-17e9-462f-b910-91a5e45dfec5"],

Cell[TextData[{
 StyleBox["Theorem (",
  FontWeight->"Bold"],
 StyleBox[ButtonBox["universal quantum computation",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/tutorial/UniversalQuantumComputation#1255432361"],
  FontWeight->"Bold"],
 StyleBox[")",
  FontWeight->"Bold"],
 ": ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"H", ",", "S", ",", "T", ",", "CNOT"}], "}"}], TraditionalForm]],
  ExpressionUUID->"09e18ae9-272d-4618-80e0-88f57bee82f2"],
 " is a universal set of gates for quantum computation."
}], "Text",
