Cell[TextData[{
 StyleBox["Convention",
  FontWeight->"Bold"],
 ": ",
 StyleBox["Q3", "Input"],
 " refers to the eigenvalues \[PlusMinus]1 of a Pauli string by binary digits \
0 and 1, respectively;"
}], "Text",
 CellChangeTimes->{{3.915424664274634*^9, 3.915424675439748*^9}},
 CellID->1222873370,ExpressionUUID->"204bc39e-a271-4ab2-b963-94a66ec24699"],

