 CellID->1950186126,ExpressionUUID->"7db17e3a-ff1e-46ac-abd0-b420db1b8348"],

Cell["Quantum Fourier transform (QFT)", "Item",
 CellChangeTimes->{{3.914902257394033*^9, 3.914902264094943*^9}, {
  3.9149023607327747`*^9, 3.914902369538561*^9}},
 CellID->164311846,ExpressionUUID->"15c0b146-e866-4a8d-9167-403415e76ec5"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      SubscriptBox["P", "k"]},
     "Ket"], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{"N", "-", "1"}]], 
     TemplateBox[{
       SubscriptBox["X", "x"]},
      "Ket"], " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
       RowBox[{"x", "\[VeryThinSpace]", "(", 
        RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", 
         RowBox[{"k", "/", "N"}]}], ")"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"5e189c94-4b24-45d9-81f6-44c112c38a14"],
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"k", "=", "0"}], ",", "1", ",", "2", ",", "\[Ellipsis]", ",", 
     RowBox[{"N", "-", "1"}]}], ")"}], TraditionalForm]],ExpressionUUID->
  "502643f4-b1b6-414d-bbd8-4fada1057695"]
}], "Text",
 CellChangeTimes->{{3.914901999848721*^9, 3.914902002977979*^9}, {
   3.914902059063909*^9, 3.914902192909396*^9}, 3.914902279012776*^9, {
   3.914902394802663*^9, 3.914902422467422*^9}},
 CellID->2072709327,ExpressionUUID->"dd2f1c4d-eb0d-4b5a-a2fc-09c886932f58"],

Cell["Matrix form of QFT", "Item",
 CellChangeTimes->{{3.9149039305234947`*^9, 3.9149039371425962`*^9}},
 CellID->1693420902,ExpressionUUID->"2316841d-1681-4770-a00f-97ad5a0ae69a"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["P", "0"]},
       "Ket"], ",", 
      TemplateBox[{
        SubscriptBox["P", "1"]},
       "Ket"], ",", "\[Ellipsis]"}], ")"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       TemplateBox[{
         SubscriptBox["X", "0"]},
        "Ket"], ",", 
       TemplateBox[{
         SubscriptBox["X", "1"]},
        "Ket"], ",", "\[Ellipsis]"}], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["D", "00"], 
         SubscriptBox["D", "01"], "\[Ellipsis]"},
        {
         SubscriptBox["D", "10"], 
         SubscriptBox["D", "11"], "\[Ellipsis]"},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]"}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "7545e847-640b-43c8-bf31-12b2d16790cd"],
 "  ,\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     RowBox[{"exp", "(", 
      FractionBox[
       RowBox[{
       "2", "\[Pi]", "\[VeryThinSpace]", "\[ImaginaryI]", " ", "i", 
        "\[VeryThinSpace]", "j"}], "N"], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"3cbfc74a-4590-44d0-94fd-98cf2b1fb70a"]
}], "Text",
 CellChangeTimes->{{3.9149032325044403`*^9, 3.9149033358284802`*^9}, {
  3.914903383397623*^9, 3.914903522592673*^9}, {3.91490395630933*^9, 
  3.9149040014553976`*^9}},
 CellID->724326223,ExpressionUUID->"ef8b2fef-b102-4f61-b0bf-0f5ca9198eaa"],

