Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "(", "k", ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"2", "\[Pi]"}]]], 
     RowBox[{"\[Integral]", 
      RowBox[{"dx", " ", 
       RowBox[{"f", "(", "x", ")"}], "\[VeryThinSpace]", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "x", "\[VeryThinSpace]", 
         "k"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "b118b361-1fa1-4edc-ad8c-d4ee4f48dd41"]
}], "Text",
