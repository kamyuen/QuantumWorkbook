Cell[TextData[{
 "\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "\[RightTeeArrow]", 
    RowBox[{"F", "(", "k", ")"}]}], TraditionalForm]],ExpressionUUID->
  "c9b0b6d0-ef6e-411b-8d3f-15a9df74c500"]
}], "Text",
 CellChangeTimes->{{3.914902249728258*^9, 3.914902276107258*^9}, {
  3.915482376864409*^9, 3.915482393297404*^9}, {3.915482678470313*^9, 
  3.915482681239137*^9}},
 CellID->1191486009,ExpressionUUID->"eea7bf0e-4e2d-46c5-9dcc-c0fa7cfa4c72"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "(", "k", ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"2", "\[Pi]"}]]], 
     RowBox[{"\[Integral]", 
      RowBox[{"dx", " ", 
       RowBox[{"f", "(", "x", ")"}], "\[VeryThinSpace]", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "x", "\[VeryThinSpace]", 
         "k"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "9faa64f3-1e62-46ed-a45f-42784cfcdc01"]
}], "Text",
 CellChangeTimes->{{3.914901846297113*^9, 3.91490192806854*^9}, {
