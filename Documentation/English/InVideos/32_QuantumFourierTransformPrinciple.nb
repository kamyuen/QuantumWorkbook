Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "k"], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{"N", "-", "1"}]], 
     SubscriptBox["f", "x"], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
       RowBox[{"x", "\[VeryThinSpace]", "(", 
        RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", 
         RowBox[{"k", "/", "N"}]}], ")"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"21a44e1b-49ea-458c-b890-abdd7e22d99c"],
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"k", "=", "0"}], ",", "1", ",", "2", ",", "\[Ellipsis]", ",", 
     RowBox[{"N", "-", "1"}]}], ")"}], TraditionalForm]],ExpressionUUID->
  "a51c5231-b1ac-4fd9-ba43-047d2ee22c2f"]
}], "Text",
