Cell[TextData[{
 "\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["f", "x"], "|", "x"}], "=", "0"}], ",", "1", ",", "2", 
      ",", "\[Ellipsis]", ",", 
      RowBox[{"N", "-", "1"}]}], "}"}], "\[RightArrow]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["F", "k"], "|", "k"}], "=", "0"}], ",", "1", ",", "2", 
       ",", "...", ",", "N"}], "}"}], "-", "1"}]}], TraditionalForm]],
  ExpressionUUID->"0683b136-3afc-4610-8adc-d4ef16d7e441"]
}], "Text",
 CellChangeTimes->{{3.914902257394033*^9, 3.914902264094943*^9}, {
  3.915482399689146*^9, 3.915482453410646*^9}, {3.915482684607763*^9, 
  3.915482693285243*^9}},
 CellID->1117138768,ExpressionUUID->"65649cb6-3c20-4921-a695-5dcc3ecfc8e3"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "k"], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{"N", "-", "1"}]], 
     SubscriptBox["f", "x"], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
       RowBox[{"x", "\[VeryThinSpace]", "(", 
        RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", 
         RowBox[{"k", "/", "N"}]}], ")"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"f61efce0-1444-4794-9965-bfc6895295e5"]
}], "Text",
