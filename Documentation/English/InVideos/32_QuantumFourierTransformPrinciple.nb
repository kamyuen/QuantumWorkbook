Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["F", "0"], ",", 
      SubscriptBox["F", "1"], ",", "\[Ellipsis]"}], ")"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["f", "0"], ",", 
       SubscriptBox["f", "1"], ",", "\[Ellipsis]"}], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["D", "00"], 
         SubscriptBox["D", "01"], "\[Ellipsis]"},
        {
         SubscriptBox["D", "10"], 
         SubscriptBox["D", "11"], "\[Ellipsis]"},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]"}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "16ddf0fc-a0ae-48af-abcd-b7848eec8fcf"],
 "  ,\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["N"]], 
     RowBox[{"exp", "(", 
      FractionBox[
       RowBox[{
       "2", "\[Pi]", "\[VeryThinSpace]", "\[ImaginaryI]", " ", "i", 
        "\[VeryThinSpace]", "j"}], "N"], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"5235c553-12c7-4c4a-be2b-e52e613cad79"]
}], "Text",
