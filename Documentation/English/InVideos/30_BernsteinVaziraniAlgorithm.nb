      RowBox[{"S", "@", "all"}]}], "]"}], ",", 
    RowBox[{"S", "[", 
     RowBox[{"all", ",", "6"}], "]"}], ",", 
    RowBox[{"Oracle", "[", 
     RowBox[{"f", ",", 
      RowBox[{"S", "@", "cc"}], ",", 
      RowBox[{"S", "@", "tt"}]}], "]"}], ",", 
    RowBox[{"S", "[", 
     RowBox[{"all", ",", "6"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<Invisible\>\"", "->", 
     RowBox[{"S", "@", 
      RowBox[{"{", "2.5", "}"}]}]}]}], "]"}]}]}], "Input",
