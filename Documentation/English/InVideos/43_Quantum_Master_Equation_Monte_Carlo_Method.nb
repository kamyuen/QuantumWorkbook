Cell[TextData[{
 "Integrating quantum master equation gets quickly intractable as the system \
increases in size. For example, for a system of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "72d7158a-15ae-4edc-b1cf-d62e4bc60353"],
 " qubits, one has to deal with an order of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["4", "n"], "\[Times]", 
    SuperscriptBox["4", "n"]}], TraditionalForm]],ExpressionUUID->
  "f9f45da9-e9ea-47f3-bd2f-29c40c9c3d31"],
 "matrix. The required memory is simply too large for most available \
computers."
}], "Text",
 CellChangeTimes->{{3.8944886568806868`*^9, 3.89448866862607*^9}},
 CellID->2086703375,ExpressionUUID->"05cf6985-2d1e-41db-b2e6-854ac0429c2b"],

Cell[TextData[{
 "For a short elapse ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"a7273c18-0296-4898-8050-682376fb5fbb"],
 " of time from ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]],ExpressionUUID->
  "58aa465c-a26e-48df-9c9a-0c9f6d5b43f6"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "+", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], TraditionalForm]],
  ExpressionUUID->"9c8743b2-fd12-44dd-9543-f029bab20b52"],
 ", up to the first order in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"43ebf9da-2df7-436b-bca3-36b2a15aab4d"],
 ", the density operator evolves as"
}], "Text",
 CellChangeTimes->{3.894488671533061*^9},
 CellID->668184468,ExpressionUUID->"dc923556-62f3-4d0a-ab77-79ec20bf7a4a"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["U", "non"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      RowBox[{
       SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"e98228f4-e3ed-4f72-923b-a2ce4701f007"],
 " ,"
}], "Text",
