      RowBox[{"Sqrt", "[", 
       RowBox[{"$p", "/", "3"}], "]"}], "*", 
      RowBox[{"S", "[", "1", "]"}]}], ",", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"$p", "/", "3"}], "]"}], "*", 
      RowBox[{"S", "[", "2", "]"}]}], ",", 
