Cell[TextData[{
 "The system undergoes an incoherent process with probability ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]],ExpressionUUID->
  "11556600-5232-4a61-9445-fa734318f429"],
 " and remains intact with probability ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[Minus]", "p"}], TraditionalForm]],ExpressionUUID->
  "d8335341-5298-491f-86f1-9ebb2afd6c3b"],
 "."
}], "Item",
 CellChangeTimes->{
  3.915074598790511*^9, {3.91507463871161*^9, 3.915074648539038*^9}, {
   3.915571706089297*^9, 3.915571716663042*^9}},
 CellID->513854521,ExpressionUUID->"dbcbcf03-2c55-476b-b902-7d581f936307"],

Cell[TextData[{
 "The incoherent process may cause the bit-flip, phase-flip, or both with \
equal probability ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "/", "3"}], TraditionalForm]],ExpressionUUID->
  "935c7fb1-f682-4905-927b-9176928d25a3"],
