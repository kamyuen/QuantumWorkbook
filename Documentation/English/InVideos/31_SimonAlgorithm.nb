Cell[TextData[{
 "For all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", ",", 
    RowBox[{"y", "\[Element]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"b57b5a87-7ec4-4806-8980-87cda43739e7"],
