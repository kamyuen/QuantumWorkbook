 CellID->776887413,ExpressionUUID->"f9b13bd0-63cf-4d76-a37f-4d2a4c388bc6"],

Cell[TextData[{
 "For all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", ",", 
    RowBox[{"y", "\[Element]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"a90180a5-0dd8-47bb-9f6f-037591e3ad4e"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{"f", 
     RowBox[{"(", "y", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "77551c64-2f87-4cba-9846-c8ae1e63dd48"],
 " if and only if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"x", "\[CirclePlus]", "s"}]}], TraditionalForm]],ExpressionUUID->
  "e6aaf84e-8d4e-47ca-820a-bb06faae0ae7"],
 ". Note that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "89769af6-2b86-4719-878f-75b9c48a3093"],
 " is either one-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "5d9fb4ca-dbe4-4d8c-819b-734dac4c228e"],
 ") or two-to-one (",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "d3fc620e-42fb-4331-879d-49aa8be39273"],
 "). "
}], "ItemNumbered",
 CellChangeTimes->{{3.852070435901077*^9, 3.852070716088048*^9}, 
