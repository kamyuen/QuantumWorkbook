Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"0"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"0"},
      "Ket"]}], 
    OverscriptBox["\[RightArrow]", 
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]]], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     RowBox[{
      TemplateBox[{"x"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"0"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "ad0b8063-1156-4556-bf03-c5acc0d62a58"],
 "."
}], "Text",
