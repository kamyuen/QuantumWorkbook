  FormBox[
   RowBox[{
    RowBox[{"H", "(", 
     RowBox[{"X", ":", "Y"}], " ", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"H", "(", "X", ")"}], "\[Minus]", 
      RowBox[{"H", "(", 
       RowBox[{"X", "|", "Y"}], " ", ")"}]}], "\[Congruent]", 
     RowBox[{
      RowBox[{"H", "(", "Y", ")"}], "-", 
      RowBox[{"H", "(", 
