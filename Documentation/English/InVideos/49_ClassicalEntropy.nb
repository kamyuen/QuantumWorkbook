Cell["\<\
Consider an evil dice with strongly biased probability distribution.\
\>", "Text",
 CellChangeTimes->{{3.871672877690449*^9, 3.87167289833857*^9}, {
  3.871673056939376*^9, 3.871673065267537*^9}, {3.871673173456792*^9, 
  3.871673173613982*^9}, {3.871673536580573*^9, 3.871673546436203*^9}, {
  3.871673591540234*^9, 3.871673624260055*^9}},
 CellID->19784877,ExpressionUUID->"43929c7b-7e66-414a-96ac-24ff00df9f49"],

Cell[BoxData[
 RowBox[{"pp", "=", 
  RowBox[{"Normalize", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"1", ",", "2", ",", "2", ",", "4", ",", "3", ",", "0"}], "}"}], 
    ",", "Total"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.871673398291647*^9, 3.871673477559021*^9}},
