 CellID->205472260,ExpressionUUID->"7fbfd20f-f759-4a01-89a6-dff7f46450f7"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"H", "(", 
      RowBox[{"X", ",", "Y"}], " ", ")"}], "\[Congruent]", 
     RowBox[{"H", "(", 
      RowBox[{"{", 
       RowBox[{"p", "(", 
        RowBox[{"x", ",", " ", "y"}], ")"}], "}"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{"\[Minus]", 
      UnderscriptBox["\[CapitalSigma]", 
       RowBox[{"x", ",", "y"}]]}], 
     RowBox[{"p", "(", 
      RowBox[{"x", ",", "y"}], ")"}], "log", "\[VeryThinSpace]", 
     RowBox[{"p", "(", 
      RowBox[{"x", ",", "y"}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "afcf0dd9-23bd-46a9-8f72-8f29665b369a"],
 "."
}], "Text",
 CellChangeTimes->{{3.908746872410018*^9, 3.908746929549952*^9}},
