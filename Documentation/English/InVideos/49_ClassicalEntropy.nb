Cell[BoxData[
 RowBox[{"qq", "=", 
  RowBox[{"Normalize", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"1", ",", "3", ",", "2", ",", "4", ",", "1", ",", "1"}], "}"}], 
    ",", "Total"}], "]"}]}]], "Input",
 CellLabel->"In[61]:=",
 CellID->828444533,ExpressionUUID->"0d11b40b-fd19-40c5-96be-14b23e388657"],

