Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"H", "(", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"p", "(", "x", ")"}], "}"}], "\[DoubleVerticalBar]", 
      RowBox[{"{", 
       RowBox[{"q", "(", "x", ")"}], "}"}]}], ")"}], ":=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "x"], 
      RowBox[{"p", "(", "x", ")"}], " ", "log", " ", 
      FractionBox[
       RowBox[{"p", "(", "x", ")"}], 
       RowBox[{"q", "(", "x", ")"}]]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"H", "(", 
        RowBox[{"{", 
         RowBox[{"p", "(", "x", ")"}], "}"}], ")"}]}], "-", 
      RowBox[{
       UnderscriptBox["\[CapitalSigma]", "x"], 
       RowBox[{"p", "(", "x", ")"}], "log", "\[VeryThinSpace]", 
       RowBox[{"q", "(", "x", ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "5e5774e9-7f8b-4fae-ba92-c7c904b2b263"],
 "."
}], "Text",
