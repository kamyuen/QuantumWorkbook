    RowBox[{"p", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"FrameLabel", "->", 
    RowBox[{"{", 
     RowBox[{"\"\<p\>\"", ",", "\"\<S(\[Rho])\>\""}], "}"}]}]}], 
  "]"}]], "Input",
