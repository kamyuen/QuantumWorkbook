Cell[CellGroupData[{

Cell["Example", "Subsection",
 CellChangeTimes->{{3.915657477442595*^9, 3.915657478505306*^9}},
 CellID->790454356,ExpressionUUID->"5886c154-86b6-44be-8d7a-3b3a6c8580d4"],

Cell[TextData[{
 "Consider a density operator describes an ensemble of two pure states ",
 Cell[BoxData[
  TemplateBox[{"0"},
   "Ket"]],ExpressionUUID->"caeb712f-32ff-4567-a6d6-53922d999711"],
 " and ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{"+"},
    "Ket"], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      TemplateBox[{"0"},
       "Ket"], "+", 
      TemplateBox[{"1"},
       "Ket"]}], ")"}], "/", 
    SqrtBox["2"]}]}]],ExpressionUUID->
  "395447bd-f0fb-4754-879a-a2d71d1e3cef"],
 "."
}], "Text",
 CellChangeTimes->{{3.83926730377583*^9, 3.839267385558619*^9}, {
  3.839267675995208*^9, 3.839267689437354*^9}, {3.915657486189394*^9, 
  3.915657495432292*^9}},
 CellID->1058952787,ExpressionUUID->"e70faf71-07cc-4870-a884-e09b56e8ba08"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "p", "]"}], "\[IndentingNewLine]", 
 RowBox[{"ens", "=", 
  RowBox[{"Association", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Ket", "[", 
      RowBox[{"S", "->", "0"}], "]"}], "->", "p"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Ket", "[", 
         RowBox[{"S", "->", "0"}], "]"}], "+", 
        RowBox[{"Ket", "[", 
         RowBox[{"S", "->", "1"}], "]"}]}], ")"}], "/", 
      RowBox[{"Sqrt", "[", "2", "]"}]}], "->", 
     RowBox[{"1", "-", "p"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellChangeTimes->{{3.8392673923570023`*^9, 3.839267489503368*^9}, {
  3.885091322327434*^9, 3.8850913315384407`*^9}, {3.885091406706219*^9, 
  3.885091408333132*^9}, {3.915657529451626*^9, 3.915657531845009*^9}, {
  3.915657563292228*^9, 3.915657625671658*^9}},
 CellLabel->"In[120]:=",
 CellID->1622508420,ExpressionUUID->"0c80263c-358a-4e22-a4d8-599ce57f7f9e"],

Cell[BoxData[
 RowBox[{"rho", "=", 
  RowBox[{"Total", "@", 
   RowBox[{"KeyValueMap", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"#1", "**", 
       RowBox[{"Dagger", "[", "#1", "]"}], "**", "#2"}], "&"}], ",", "ens"}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.915657642934033*^9, 3.915657663137305*^9}},
 CellLabel->"In[127]:=",
 CellID->1331036665,ExpressionUUID->"3e391a3b-595f-4047-9814-8f08abd4df66"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Matrix", "[", "rho", "]"}], "//", "MatrixForm"}]], "Input",
 CellChangeTimes->{{3.915657667612894*^9, 3.915657671976132*^9}},
 CellLabel->"In[128]:=",
 CellID->358687412,ExpressionUUID->"4c8f4c2b-b36e-44aa-8beb-32cefa73196f"],

Cell["Examine the eigenvalues of the density operator.", "Text",
 CellChangeTimes->{{3.839267623516022*^9, 3.839267632954385*^9}, {
  3.91565751093014*^9, 3.915657514384901*^9}},
 CellID->751665334,ExpressionUUID->"0359f762-aa41-4bd8-a40f-b094f0016518"],

Cell[BoxData[
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"ProperValues", "[", "rho", "]"}], ",", 
