Cell[TextData[{
 "Consider a quantum register of qubits, referred to by symbol ",
 StyleBox["S", "Input"],
 "."
}], "Text",
 CellChangeTimes->{{3.88553184867208*^9, 3.885531910780252*^9}},
 CellID->947914214,ExpressionUUID->"e7564d89-2dd7-49eb-ba40-8a2796cdd9e5"],

Cell[BoxData[
 RowBox[{"Let", "[", 
  RowBox[{"Qubit", ",", "S"}], "]"}]], "Input",
 CellChangeTimes->{{3.885531832914682*^9, 3.885531836016836*^9}},
 CellLabel->"In[388]:=",
 CellID->1987100488,ExpressionUUID->"09fc3ac3-5768-4a62-a699-1fef0236c35e"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$L", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"ss", "=", 
  RowBox[{"S", "[", 
   RowBox[{
    RowBox[{"Range", "@", "$L"}], ",", "$"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.885531918602758*^9, 3.885531951594625*^9}},
 CellLabel->"In[389]:=",
 CellID->492158006,ExpressionUUID->"96b0ee28-a55c-42f8-919a-13e6fe4748e4"],

Cell["\<\
Here is a dynamic way of constructing the spin XX Hamiltonian.\
\>", "Text",
 CellChangeTimes->{{3.885597875180862*^9, 3.885597896652978*^9}, {
  3.885597927371285*^9, 3.8855979291610518`*^9}},
 CellID->1943187986,ExpressionUUID->"051151c7-b5db-4c6f-a117-4ce915d7bd2b"],

Cell[BoxData[
 RowBox[{"xx", "=", 
  RowBox[{"Total", "@", 
   RowBox[{"ChainBy", "[", 
    RowBox[{
     RowBox[{"Through", "[", 
      RowBox[{"ss", "[", "1", "]"}], "]"}], ",", "Multiply"}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.885531973154882*^9, 3.885531999793967*^9}, {
  3.885535676706672*^9, 3.885535677176016*^9}},
 CellLabel->"In[391]:=",
 CellID->1349058495,ExpressionUUID->"63f3737a-00dd-4655-9a4e-f7350322b2c4"],

Cell["In the same manner, construct the spin YY Hamiltonian.", "Text",
 CellChangeTimes->{{3.885597875180862*^9, 3.885597935142394*^9}},
 CellID->868357275,ExpressionUUID->"e504e148-a630-46f4-8fad-fe115e898594"],

Cell[BoxData[
 RowBox[{"yy", "=", 
  RowBox[{"Total", "@", 
   RowBox[{"ChainBy", "[", 
    RowBox[{
     RowBox[{"Through", "[", 
      RowBox[{"ss", "[", "2", "]"}], "]"}], ",", "Multiply"}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.8855356827692947`*^9, 3.885535694028392*^9}},
 CellLabel->"In[394]:=",
 CellID->378388430,ExpressionUUID->"9924233f-5c32-4e85-80f1-23a4298acfa6"],

Cell["Finally, construct the spin ZZ Hamiltonian.", "Text",
 CellChangeTimes->{{3.885597939844143*^9, 3.885597950350137*^9}},
 CellID->1659670337,ExpressionUUID->"7dfa8ab8-1b90-44b1-82b9-0e9a5a641ac5"],

Cell[BoxData[
 RowBox[{"zz", "=", 
  RowBox[{"Total", "@", 
   RowBox[{"ChainBy", "[", 
    RowBox[{
     RowBox[{"Through", "[", 
      RowBox[{"ss", "[", "3", "]"}], "]"}], ",", "Multiply"}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.885535688455616*^9, 3.885535697451099*^9}},
 CellLabel->"In[395]:=",
 CellID->1276214582,ExpressionUUID->"2c4ff269-184e-443b-8804-a4b1ec0e4861"],

Cell["Here is the total Hamiltonian.", "Text",
 CellChangeTimes->{{3.88559795482012*^9, 3.885597960715541*^9}},
 CellID->273627672,ExpressionUUID->"fc29f28a-29a3-42c0-984b-05eac543a984"],

Cell[BoxData[
 RowBox[{"H", "=", 
  RowBox[{"xx", "+", "yy", "+", "zz"}]}]], "Input",
 CellChangeTimes->{{3.885535701357894*^9, 3.88553570490489*^9}},
 CellLabel->"In[396]:=",
 CellID->1394717291,ExpressionUUID->"e180796d-2633-4d59-a638-1db3b34bb4b9"],

Cell[TextData[{
 "The Hamiltonian has so many zeros. That is why matrix representations are \
treated in ",
 StyleBox["SparseArray", "Input"],
 " form in Q3."
}], "Text",
 CellChangeTimes->{{3.885535820844578*^9, 3.885535874359331*^9}},
 CellID->1380160947,ExpressionUUID->"c0317522-8d3b-444b-8a82-2c1b6fa43c53"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"mat", "=", 
   RowBox[{"Matrix", "[", "H", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"mat", "//", "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.885535763390786*^9, 3.885535773319025*^9}},
 CellLabel->"In[399]:=",
 CellID->1641325153,ExpressionUUID->"eec93b78-4b19-40cf-ac3b-107b6259e690"],

Cell["Anyway, how can we diagonalize Hamiltonian?", "Text",
 CellChangeTimes->{{3.885535884216138*^9, 3.885535904035636*^9}},
 CellID->959011148,ExpressionUUID->"26541cee-2f05-409c-8830-e2a6d8067220"]
}, Open  ]]
}, Closed]],

