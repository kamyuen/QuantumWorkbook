  3.850960023026443*^9, {3.850960114004863*^9, 3.850960130305483*^9}, {
   3.899197978692188*^9, 3.89919798734822*^9}},
 CellID->26346415,ExpressionUUID->"df8e883f-c4dd-4ff6-ad69-609a37a24745"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"x", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      SuperscriptBox[
       RowBox[{
        TemplateBox[{"x"},
         "Ket"], "(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"x", "\[CenterDot]", "s"}]]}], "\[RightArrow]", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"y", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      TemplateBox[{"y"},
       "Ket"], 
      SuperscriptBox[
       RowBox[{
        UnderoverscriptBox["\[CapitalSigma]", 
         RowBox[{"x", "=", "0"}], 
         RowBox[{
          SuperscriptBox["2", "n"], "-", "1"}]], "(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{
        RowBox[{"x", "\[CenterDot]", "s"}], "+", 
        RowBox[{"x", "\[CenterDot]", "y"}]}]]}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"y", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      TemplateBox[{"y"},
       "Ket"], 
      SuperscriptBox[
       RowBox[{
        UnderoverscriptBox["\[CapitalSigma]", 
         RowBox[{"x", "=", "0"}], 
         RowBox[{
          SuperscriptBox["2", "n"], "-", "1"}]], "(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"x", "\[CenterDot]", 
        RowBox[{"(", 
         RowBox[{"s", "\[CirclePlus]", "y"}], ")"}]}]]}], "=", 
     TemplateBox[{"s"},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "dba15348-8724-4a59-ac0a-1e2e1d71bd47"]
}], "Text",
