Cell["\<\
Now, suppose that we want to encode a single-qubit state to protect it \
against the bit-flip and phase-flip errors.\
\>", "Text",
 CellID->646562821,ExpressionUUID->"1098b9c4-6246-4bba-9da5-e0e1e224021a"],

Cell[BoxData[
 RowBox[{"in", "=", 
  RowBox[{"ProductState", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"S", "[", "1", "]"}], "->", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c", "[", "0", "]"}], ",", 
       RowBox[{"c", "[", "1", "]"}]}], "}"}]}], ",", 
    RowBox[{"\"\<Label\>\"", "->", 
     RowBox[{"Ket", "[", "\[Psi]", "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.832292719756838*^9, 3.832292766888834*^9}, {
  3.832292887817581*^9, 3.832292893028821*^9}, {3.882212465403815*^9, 
  3.882212465444929*^9}},
 CellLabel->"In[87]:=",
 CellID->514078727,ExpressionUUID->"8bd2e111-4e16-4276-b2e1-85a917113549"],

Cell["Calculate the output state.", "Text",
 CellChangeTimes->{{3.9082549414367948`*^9, 3.9082549551026278`*^9}},
 CellID->843946282,ExpressionUUID->"bcdecdd3-1ec8-41a4-b081-506bc7e1eb0e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"out", "=", 
   RowBox[{"qc3", "**", "in"}]}], ";"}]], "Input",
 CellChangeTimes->{3.908254925038618*^9},
 CellLabel->"In[114]:=",
 CellID->1303105708,ExpressionUUID->"08dab736-6686-4c57-ac41-52c95736e9d3"],

Cell["Extract the observables in the measurements.", "Text",
 CellChangeTimes->{{3.908254960783494*^9, 3.908254977171606*^9}},
 CellID->1350242134,ExpressionUUID->"bc4030b7-c9c5-4ae5-84dc-b38e7ffd7c90"],

Cell[BoxData[
 RowBox[{"gnr", "=", 
  RowBox[{"Flatten", "[", 
   RowBox[{"msr", "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"QuantumCircuit", "->", "List"}], ",", 
      RowBox[{"Measurement", "->", "Identity"}], ",", 
      RowBox[{"\"\<Spacer\>\"", "->", "Nothing"}]}], "}"}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.9082547643341837`*^9, 3.908254851859928*^9}},
 CellLabel->"In[115]:=",
 CellID->851138189,ExpressionUUID->"eda91b34-55c8-4f3f-8c8d-9801addd6c94"],

Cell["\<\
Check the measurement outcomes, which tells you where the error occurs and \
what is the error.\
\>", "Text",
 CellChangeTimes->{{3.90825498563*^9, 3.908254992065734*^9}, {
  3.908255047694637*^9, 3.908255070389843*^9}},
 CellID->1492667055,ExpressionUUID->"4f9f7bf6-7cfe-4322-8ced-42e9313f7d5c"],

Cell[BoxData[
 RowBox[{"sdr", "=", 
  RowBox[{"Readout", "/@", "gnr"}]}]], "Input",
 CellChangeTimes->{{3.882230557904427*^9, 3.88223056846772*^9}, {
   3.882230769187293*^9, 3.882230849062951*^9}, {3.8822308894697733`*^9, 
