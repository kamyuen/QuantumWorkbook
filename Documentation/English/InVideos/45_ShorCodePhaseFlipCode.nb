  ";"}], "\[IndentingNewLine]", 
 RowBox[{"KetFactor", "@", "ket1"}]}], "Input",
 CellChangeTimes->{{3.832284979605987*^9, 3.832285014615176*^9}, {
   3.832285047308599*^9, 3.832285058174313*^9}, {3.832285228451855*^9, 
   3.832285248059773*^9}, 3.839727161681483*^9, {3.882229728847878*^9, 
   3.882229793402789*^9}, {3.882230005727479*^9, 3.88223000752472*^9}, {
   3.895531163221488*^9, 3.895531164537016*^9}},
 CellLabel->"In[40]:=",
 CellID->758091126,ExpressionUUID->"da43e4b2-37b9-48a1-abfe-0e7b45835f76"],

Cell[TextData[{
 "This is the set of errors against which we can protect our state with the \
nine-qubit code. Note that included are the Pauli Y operators, which refer to \
the case of ",
 StyleBox["simultaneous",
  FontSlant->"Italic"],
 " bit-flip and phase-flip errors on the same qubit."
}], "Text",
 CellChangeTimes->{{3.882230130136553*^9, 3.882230169170849*^9}, {
  3.895531817584857*^9, 3.895531924361652*^9}},
 CellID->805298593,ExpressionUUID->"d61eb393-84e5-4179-9028-162a78ce9c6d"],

Cell[BoxData[
 RowBox[{"errors", "=", 
  RowBox[{"Join", "[", 
   RowBox[{
    RowBox[{"S", "[", 
     RowBox[{"kk", ",", "1"}], "]"}], ",", 
    RowBox[{"S", "[", 
     RowBox[{"kk", ",", "3"}], "]"}], ",", 
    RowBox[{"S", "[", 
     RowBox[{"kk", ",", "2"}], "]"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.882230077194684*^9, 3.882230100333053*^9}, {
  3.895531603647517*^9, 3.895531608958558*^9}, {3.895531656234898*^9, 
  3.895531668292621*^9}},
 CellLabel->"In[42]:=",
 CellID->1565278019,ExpressionUUID->"a3fe60cb-6646-4fcb-a90b-9a61cf96e035"],

Cell["Randomly choose an error from the above set.", "Text",
 CellChangeTimes->{{3.894771564899444*^9, 3.8947716021718397`*^9}},
 CellID->1001833771,ExpressionUUID->"4a0542ff-7dac-4b81-881b-c63811f1c5c9"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"err", "=", 
   RowBox[{"RandomChoice", "[", "errors", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PauliForm", "[", 
   RowBox[{"err", ",", 
    RowBox[{"S", "@", 
     RowBox[{"Range", "@", "9"}]}]}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"qc2", "=", 
  RowBox[{"QuantumCircuit", "[", 
   RowBox[{"qc", ",", "\"\<Separator\>\"", ",", "err"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.882230034792153*^9, 3.882230049186202*^9}, {
  3.8822301238364964`*^9, 3.8822301241597767`*^9}, {3.885022094848597*^9, 
  3.885022098609085*^9}, {3.895531615663205*^9, 3.895531627501141*^9}},
 CellLabel->"In[43]:=",
 CellID->1107026390,ExpressionUUID->"63e6a665-3679-4a15-8990-71b4fef1f396"],

Cell["Construct the measurements for error-syndrome detection.", "Text",
 CellChangeTimes->{{3.894771496949248*^9, 3.894771513361337*^9}},
 CellID->1552813743,ExpressionUUID->"3a714748-3af5-487a-ba51-15206c46f81f"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"msr", "=", 
   RowBox[{"QuantumCircuit", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"QuantumCircuit", "@@@", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Measurement", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"1", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"2", ",", "3"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"2", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"3", ",", "3"}], "]"}]}]}], "}"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Measurement", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"4", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"5", ",", "3"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"5", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"6", ",", "3"}], "]"}]}]}], "}"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Measurement", "/@", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"7", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"8", ",", "3"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"S", "[", 
             RowBox[{"8", ",", "3"}], "]"}], "**", 
            RowBox[{"S", "[", 
             RowBox[{"9", ",", "3"}], "]"}]}]}], "}"}]}]}], 
       "\[IndentingNewLine]", "}"}]}], ",", 
     RowBox[{"{", "\"\<Spacer\>\"", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Measurement", "[", 
      RowBox[{"Multiply", "@@", 
       RowBox[{"S", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"1", ",", "6"}], "]"}], ",", "1"}], "]"}]}], "]"}], ",", 
     RowBox[{"{", "\"\<Spacer\>\"", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Measurement", "[", 
      RowBox[{"Multiply", "@@", 
       RowBox[{"S", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"4", ",", "9"}], "]"}], ",", "1"}], "]"}]}], "]"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"qc3", "=", 
  RowBox[{"QuantumCircuit", "[", 
   RowBox[{"qc2", ",", "\"\<Separator\>\"", ",", "msr"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.882230189185862*^9, 3.882230191336282*^9}, {
   3.8822302235267878`*^9, 3.882230546350757*^9}, {3.882230628321428*^9, 
   3.882230736621079*^9}, 3.894771470878801*^9, {3.8947716351489773`*^9, 
   3.894771635521565*^9}, {3.895531686976773*^9, 3.895531733545382*^9}, {
   3.90825413896343*^9, 3.908254196886351*^9}, {3.908254243371035*^9, 
   3.908254253984711*^9}, {3.908254340384959*^9, 3.908254394726143*^9}, {
