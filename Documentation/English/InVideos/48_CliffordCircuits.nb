 CellID->591332509,ExpressionUUID->"b95c56ea-96fa-4ba3-b04b-1c0ad6fbfe5a"],

Cell[BoxData[
 RowBox[{"qc", "=", 
  RowBox[{"QuantumCircuit", "[", 
   RowBox[{"Sequence", "@@", 
    RowBox[{"Reverse", "@", "gates"}]}], "]"}]}]], "Input",
