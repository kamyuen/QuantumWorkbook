Cell[CellGroupData[{

Cell["Clifford Operators", "Section",
 CellChangeTimes->{{3.9145505215338907`*^9, 3.914550524555667*^9}},
 CellID->1230124648,ExpressionUUID->"c56b2418-0ad6-4295-b4ac-a49146098a06"],

Cell[TextData[{
 StyleBox["Definition (Pauli string)",
  FontWeight->"Bold"],
 ": A tensor product of single-qubit Pauli operators (such as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Z", "\[CircleTimes]", "Z", "\[CircleTimes]", "X"}], 
    "\[VeryThinSpace]", "\[Ellipsis]"}], TraditionalForm]],ExpressionUUID->
  "a23f0c78-e376-470a-be30-a7d41d93455e"],
 ") is called a ",
 StyleBox["Pauli string",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.915424522812743*^9, 3.915424565602877*^9}, {
  3.915651993669158*^9, 3.915651996905614*^9}},
 CellID->2042336787,ExpressionUUID->"c45877da-11c4-4b1b-9ecc-c5fd763d391f"],

Cell[TextData[{
 StyleBox["Definition (Clifford operator)",
  FontWeight->"Bold"],
 ": A unitary operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "2b625996-c162-4bb9-9634-8f62679e27cb"],
 " is called a ",
 StyleBox["Clifford operator",
  FontSlant->"Italic"],
 " if it transforms a Pauli string to another Pauli string."
}], "Text",
 CellChangeTimes->{{3.915424195597692*^9, 3.915424245494551*^9}, {
  3.915652000215826*^9, 3.9156520066375523`*^9}},
 CellID->1488032392,ExpressionUUID->"08d17474-c367-44cc-8318-ac8c96899e16"],

Cell[TextData[{
 StyleBox["Theorem",
  FontWeight->"Bold"],
 ": Any Clifford operator may be generated by combining ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "ebd06fc7-c4c1-4ae3-bc24-77a9e8507a53"],
 ", ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "48268349-286d-4165-9076-222648637ffb"],
 ", and CNOT."
}], "Text",
 CellChangeTimes->{{3.915424267222658*^9, 3.915424333899441*^9}, {
  3.915652011672028*^9, 3.915652015292905*^9}},
 CellID->2036002162,ExpressionUUID->"7eae2755-99d0-4d32-8010-13c263293ab2"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["2"]], 
     RowBox[{"(", GridBox[{
        {"1", "1"},
        {"1", 
         RowBox[{"-", "1"}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "39a3037b-4c85-42c5-bd18-3b20c11778dc"],
 ",     ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", ":=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"1", "0"},
        {"0", "\[ImaginaryI]"}
       }], ")"}], "=", 
     SqrtBox["Z"]}]}], TraditionalForm]],ExpressionUUID->
  "f1b456b9-22eb-4b2f-9320-45546735e043"],
 " ."
}], "Text",
 CellChangeTimes->{{3.915424337096229*^9, 3.9154244038991537`*^9}, {
  3.915651869437584*^9, 3.9156518722218122`*^9}},
 CellID->1659561479,ExpressionUUID->"a3531066-bd42-4f0b-85af-34533614ef3e"]
}, Closed]],

Cell[CellGroupData[{

Cell["Clifford Decomposition", "Section",
 CellID->1304605151,ExpressionUUID->"96d43797-ac69-4eb1-a42b-15b5a14d9b13"],

Cell[BoxData[
 RowBox[{"Let", "[", 
  RowBox[{"Qubit", ",", "S"}], "]"}]], "Input",
 CellChangeTimes->{{3.894769702260118*^9, 3.894769706893321*^9}},
 CellLabel->"In[1]:=",
 CellID->1630290610,ExpressionUUID->"7ca3f79e-5a5f-489c-8b46-460e4a0c5ca8"],

Cell["Start with a small number of qubits.", "Text",
 CellChangeTimes->{{3.881613658403053*^9, 3.881613676921496*^9}},
 CellID->1338536550,ExpressionUUID->"0dbc19e2-6045-4136-b2f1-35c6fefd2987"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$n", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"ss", "=", 
  RowBox[{"S", "[", 
   RowBox[{
    RowBox[{"Range", "@", "$n"}], ",", "None"}], "]"}]}]}], "Input",
 CellLabel->"In[2]:=",
 CellID->2092092956,ExpressionUUID->"48e2937c-a8a3-4d1e-bf9c-3faa9294280f"],

Cell[TextData[{
 "Now, choose a Clifford unitary operator. Here, we do it by choosing a ",
 ButtonBox["Gottesman matrix",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/GottesmanMatrix"],
 "."
}], "Text",
 CellChangeTimes->{{3.915652392421732*^9, 3.915652403585017*^9}},
 CellID->669790911,ExpressionUUID->"d7e2e4c2-02dd-4aa4-a224-c987d19013d4"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"bsp", "=", 
   RowBox[{"BinarySymplecticGroup", "[", "ss", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"gts", "=", 
   RowBox[{"First", "@", 
    RowBox[{"GroupElements", "[", 
     RowBox[{"bsp", ",", 
      RowBox[{"{", "2", "}"}]}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"MatrixForm", "[", "gts", "]"}]}], "Input",
 CellChangeTimes->{{3.895100609732975*^9, 3.895100696496874*^9}},
 CellLabel->"In[4]:=",
 CellID->5310392,ExpressionUUID->"c0b7d12d-18c8-4908-9bc8-c629f574393d"],

Cell["\<\
Convert the Gottesman matrix to the corresponding Clifford operator.\
\>", "Text",
 CellChangeTimes->{{3.895100747011843*^9, 3.895100772412279*^9}},
 CellID->1260019797,ExpressionUUID->"66d534d0-a8b0-4762-8ece-b8e584b268c4"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"op", "=", 
   RowBox[{"FromGottesmanMatrix", "[", 
    RowBox[{"gts", ",", "ss"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"op", "//", "PauliForm"}]}], "Input",
 CellChangeTimes->{
  3.895100609732975*^9, {3.895100721891416*^9, 3.8951007409757648`*^9}, {
   3.915422634529519*^9, 3.915422658980924*^9}},
 CellLabel->"In[7]:=",
 CellID->1790437857,ExpressionUUID->"81ada51b-640b-45c1-9ede-95093a1d6ac6"],

Cell["Check that the operator is a Clifford.", "Text",
 CellChangeTimes->{{3.8951007797800508`*^9, 3.895100785806734*^9}},
 CellID->2043432145,ExpressionUUID->"8120850c-bc00-420f-8bd3-c671d868417c"],

Cell[BoxData[
 RowBox[{"CliffordQ", "[", "op", "]"}]], "Input",
 CellChangeTimes->{
  3.895100609732975*^9, {3.895100721891416*^9, 3.8951007409757648`*^9}},
 CellLabel->"In[9]:=",
 CellID->2094042095,ExpressionUUID->"da29b898-477f-443d-97a4-f9672042589e"],

